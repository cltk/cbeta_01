": "\u5e2b\u5f92\u5eff\u8f09\u5171\u5bd2\u7070\u4e00\u7247\u5fc3\u57fa\u4ed8\u723e\u57f9\u8aaa\u8382\u672a\u63da\u5bb6\u5e95", "227": "\u53e5\u9ad8\u63d0\u7956\u5370\u7167\u838a\u676f", "228": "\u7576\u5e74\u61a4\u76e1\u5fc3\u8840\u9053\u8005\u4ee4\u4eba\u7d50\u820c\u5206\u4ed8\u4e00\u689d\u70cf\u85e4\u7559\u5f85", "229": "\u4e09\u5cf0\u61c9\u63a5", "23": "\u4e0a\u5802\u62c8\u9999\u795d\u8056\u7562\u672b\u5f8c\u62c8\u9999\u4e91\u6b64\u4e00\u74e3\u9999\u7207\u5411\u7210\u4e2d", "230": "\u8d08\u5929\u654f\u6cd5\u5b6b\u8d08\u5929\u654f\u6cd5\u5b6b", "231": "\u74e2\u7b20\u96f2\u5c71\u5ea6\u9326\u6c5f\u7a7f\u5c71\u900f\u5dba\u6162\u884c\u85cf\u8349\u978b\u8e0f\u788e\u8393\u82d4", "232": "\u77f3\u7121\u9650\u6625\u98a8\u5e36\u9732\u9999", "233": "\u793a\u7b11\u6885\u7384\u5b6b\u793a\u7b11\u6885\u7384\u5b6b", "234": "\u590f\u4e0d\u6392\u7a97\u81ea\u53d6\u6dbc\u983b\u62c8\u677e\u62c2\u58c1\u79cb\u971c\u73cd\u91cd\u9435\u5fc3\u6885\u884c", "235": "\u8005\u6709\u6642\u62d3\u51fa\u9a57\u8af8\u65b9", "236": "\u793a\u5927\u6709\u7384\u5b6b\u793a\u5927\u6709\u7384\u5b6b", "237": "\u7576\u5e74\u6703\u4f8d\u7345\u5b50\u5ea7\u4eca\u671d\u559c\u8e1e\u8c61\u738b\u5e8a\u95cd\u9ece\u535c\u7834\u4e09\u66f4", "238": "\u6708\u5207\u83ab\u9022\u4eba\u9053\u662f\u971c", "239": "\u793a\u7b11\u77f3\u793a\u7b11\u77f3", "24": "\u8011\u70ba\u73fe\u4f4f\u69ae\u7e23\u9f0e\u65b0\u5802\u4e0a\u50b3\u81e8\u6fdf\u6b63\u5b97\u7b2c\u4e09\u5341\u4e8c\u4e16", "240": "\u4e09\u4e08\u65e5\u9ad8\u758f\u8d77\u7720\u6642\u52e4\u5ba2\u4f5c\u4e00\u7aff\u7159\u9592\u651c\u7c43\u5411\u6843\u5712", "241": "\u88e1\u6eff\u8896\u9918\u9999\u6795\u9ed1\u751c", "242": "\u793a\u5929\u654f\u793a\u5929\u654f", "243": "\u5206\u660e\u89bf\u9762\u5225\u7121\u771f\u653e\u53bb\u6536\u4f86\u4e99\u53e4\u4eca\u4e0d\u5fcd\u7576\u4eba\u8f15\u89f8", "244": "\u788e\u539f\u5168\u52ff\u8ca0\u8001\u5a46\u5fc3", "245": "\u5c71\u5c45\u5c71\u5c45", "246": "\u4e0d\u6703\u4f5b\u6cd5\u4e0d\u6703\u79aa\u9951\u4f86\u55ab\u98ef\u98fd\u4f86\u7720\u7761\u5230\u4e09\u5c71\u7d05\u65e5", "247": "\u51fa\u5ba2\u81e8\u7121\u6b3e\u793a\u9ea4\u62f3", "248": "\u5e2b\u4e8e\u5eb7\u7199\u4e09\u5341\u5e74\u6b72\u5728\u8f9b\u672a\u524d\u4e03\u6708\u521d\u4e8c\u65e5\u672a\u6642\u793a\u5fae\u6059\u53ec\u773e\u5206\u5256\u5e38\u4f4f\u8a16\u8209\u7b46\u66f8\u5048\u4e91\u5e2b\u4e8e\u5eb7\u7199\u4e09\u5341\u5e74\u6b72\u5728\u8f9b\u672a\u524d\u4e03\u6708\u521d\u4e8c\u65e5\u672a", "249": "\u6642\u793a\u5fae\u6059\u53ec\u773e\u5206\u5256\u5e38\u4f4f\u8a16\u8209\u7b46\u66f8\u5048\u4e91", "25": "\u4e0a\u5927\u4e0b\u543c\u672c\u5e2b\u5927\u548c\u5c1a\u7528\u916c\u6cd5\u4e73\u4e4b\u6069\u4e0a\u9996\u767d\u69cc\u7adf", "250": "\u4e09\u5341\u5e74\u9918\u98ef\u5e97\u958b\u800c\u4eca\u6b32\u7f77\u5f85\u8ab0\u4f86\u7b0a\u7c6c\u645d\u76e1\u7121\u4e9b", "251": "\u6492\u624b\u6b78\u5bb6\u7a69\u871c\u80ce\u66f8\u7562\u7aef\u5750\u800c\u901d", "252": "\u6c34\u5143\u79aa\u5e2b\u8a9e\u9304\u7d42\u3000\u3000\u3000\u3000\u3000\u3000\u677f\u5b58\u695e\u56b4\u7d93\u574a", "26": "\u554f\u7b54\u7f77\u9a40\u8c4e\u62c4\u6756\u4e91\u4e03\u5c3a\u7cbc\udb95\udc53\u9c49\u9f3b\u86c7\u96f2\u9a30\u5b87\u5b99\u745e", "27": "\u4e09\u5df4\u689d\u689d\u7a7f\u904e\u4eba\u7121\u898b\u64bf\u9ede\u5c07\u4f86\u672a\u5230\u5bb6\u651b\u4e0b\u62c4\u6756", "28": "\u559d\u4e00\u559d\u4e0a\u9996\u7d50\u69cc\u5e2b\u4fbf\u4e0b\u5ea7\u3002", "29": "\u4e0a\u5802\u767c\u5143\u8acb\u5236\u6b64\u5fc3\u826f\u4e09\u4e03\u9577\u9023\u4e5d\u5b9a\u9999\u5750\u5230\u8fb0\u671d", "3": "\u51ac\u4f86\u83ef\u5ca9\u7d50\u5236\u6642\u4ee5\u6a5f\u92d2\u76f8\u89f8\u96d6\u7121\u4f86\u6b77\u4ea6\u4f3c\u6709\u81bd", "30": "\u4e26\u5348\u591c\u7d22\u9322\u5eab\u4e0b\u8cb7\u751f\u8591\u3002", "31": "\u4e0a\u5802\u8a00\u5982\u7bad\u6025\u638c\u5a01\u7370\u5e2b\u8c61\u8072\u54ee\u91ce\u5e72\u9a5a\u73cd\u91cd\u56db\u4f86", "32": "\u5b89\u7a69\u5750\u5288\u982d\u767d\u68d2\u8c48\u7121\u60c5\u3002", "33": "\u5929\u67f1\u5c45\u58eb\u8acb\u4e0a\u5802\u4ee5\u5750\u5177\u642d\u53f3\u80a9\u81f3\u5ea7\u8acb\u53c8\u624b\u4f5c\u5973", "34": "\u4eba\u62dc\u4e43\u965e\u5ea7\u4e91\u5ead\u524d\u767d\u65e5\u9577\u51ac\u98a8\u9001\u4e0a\u5802\u559d\u9000\u4e09\u5df4", "35": "\u6728\u68d2\u5206\u5348\u591c\u5149\u61c9\u9808\u9ad8\u8457\u773c\u60dc\u53d6\u8b1d\u4e09\u5a18\u3002", "36": "\u4e0a\u5802\u50e7\u554f\u842c\u6cd5\u6b78\u4e00\u5373\u4e0d\u554f\u5982\u4f55\u662f\u9752\u5dde\u5e03\u886b\u5e2b\u4e91", "37": "\u5343\u624b\u5927\u60b2\u63d0\u4e0d\u8d77\u9032\u4e91\u554f\u8655\u5206\u660e\u7b54\u5f97\u89aa\u5e2b\u5653\u4e00\u8072", "38": "\u4e43\u4e91\u65e5\u65e5\u4e0a\u5802\u9053\u4e0d\u4fe1\u6642\u6642\u8aaa\u5e95\u8033\u908a\u98a8\u5378\u537b\u8888\u88df", "39": "\u6b78\u820a\u4f4d\u4f86\u671d\u8fce\u9001\u53c8\u9084\u901a\u3002", "4": "\u8001\u50e7\u75db\u8207\u4e00\u9813\u6498\u537b\u4e0d\u9304\u5ffd\u53c8\u5165\u5ba4\u81ea\u8a22\u6240\u4ee5\u6094\u898b", "40": "\u4e0a\u5802\u50e7\u554f\u5982\u4f55\u662f\u6212\u5e2b\u4e91\u7a69\u53e3\u6df1\u85cf\u820c\u9032\u4e91\u5982\u4f55\u662f", "41": "\u5b9a\u5e2b\u4e91\u77e5\u6a5f\u5230\u8655\u5b89\u5982\u4f55\u662f\u6167\u5e2b\u4fbf\u6253\u554f\u91d1\u9f8d\u51fa\u73fe", "42": "\u6642\u5982\u4f55\u5e2b\u4e91\u96f2\u5f9e\u98a8\u96a8\u50e7\u4fbf\u62dc\u5e2b\u4e43\u4e91\u6709\u554f\u6709\u7b54\u9f90", "43": "\u8001\u7b0a\u7c6c\u7121\u554f\u7121\u7b54\u4e09\u5e73\u6728\u6753\u4e14\u9053\u8005\u50e7\u6041\u9ebc\u554f\u8001\u50e7", "44": "\u62c4\u6756\u6041\u9ebc\u7b54\u9084\u662f\u7b0a\u96e2\u662f\u6728\u6753\u826f\u4e45\u4e91\u83ab\u6559\u6328\u843d\u5d16", "45": "\u524d\u77f3\u6253\u7834\u4e0b\u65b9\u906e\u65e5\u96f2\u3002", "46": "\u96f2\u6d77\u7f85\u5c45\u58eb\u8acb\u4e0a\u5802\u91cd\u91cd\u64be\u9f13\u805a\u98a8\u96f2\u652a\u52d5\u9b5a\u9f8d\u5674", "47": "\u6d6a\u9a5a\u8c4e\u6a6b\u77ed\u68f9\u4eba\u7686\u8d74\u767b\u8005\u9808\u77e5\u4e0d\u4e8c\u8247\u62db\u624b\u4e91\u8d95", "48": "\u8239\u5e95\u5feb\u4f86\u62fd\u62c4\u6756\u4e0b\u5ea7\u3002", "49": "\u4f5b\u6210\u9053\u65e5\u6731\u5c45\u58eb\u8acb\u4e0a\u5802\u4e00\u68d2\u4e00\u559d\u7576\u4e0b\u9818\u7565\u4e00\u6a5f", "5": "\u4e88\u4e4b\u665a\u4e91\u53d7\u4e09\u5cf0\u8a18\u8382\u4e43\u51fa\u6240\u9304\u7a3f\u8acb\u4e88\u6b63\u5e8f\u4e88\u559c", "50": "\u4e00\u5883\u89bf\u9762\u76f8\u5448\u8005\u4e8b\u4e14\u6b62\u6628\u591c\u77bf\u66c7\u773c\u6302\u7576\u5929\u4e7e\u5764", "51": "\u768e\u6f54\u4eca\u65e5\u6731\u516c\u6bcd\u96e3\u5f97\u6b64\u826f\u8fb0\u4f9b\u4f5b\u9f4b\u50e7\u8acb\u6cd5\u5ef6\u9f61", "52": "\u6709\u4f55\u5229\u76ca\u842c\u4ede\u5ce8\u7709\u67f1\u62c8\u4f86\u4f5c\u58fd\u5c71\u3002", "53": "\u4e0a\u5802\u55ae\u63d0\u767d\u68d2\u64ad\u5b97\u98a8\u9435\u984d\u9285\u982d\u5b70\u6562\u9022\u6253\u7834\u865b\u7a7a", "54": "\u5982\u7c89\u788e\u96ea\u6d41\u5927\u5730\u6ea2\u6c5f\u6771\u3002", "55": "\u4e0a\u5802\u983b\u767b\u6a13\u95a3\u5750\u884c\u6b63\u4ee4\u83ab\u5c07\u6ce5\u5f48\u5b50\u559a\u4f5c\u7834\u6c99\u76c6", "56": "\u9760\u62c4\u6756\u4e0b\u5ea7\u3002", "57": "\u4e0a\u5802\u4e09\u5cf0\u5c71\u88e1\u65e5\u521d\u9577\u4e00\u6bb5\u6e05\u6689\u559c\u6cd5\u5802\u52a9\u6211\u4e5f\u7121", "58": "\u4e9b\u5b50\u529b\u6276\u6301\u62c4\u6756\u592a\u90ce\u7576\u3002", "59": "\u89e3\u5236\u4e0a\u5802\u767e\u65e5\u671f\u5713\u6709\u59cb\u7d42\u83ab\u8f15\u8a9e\u9ed8\u5feb\u76f8\u9022\u9ad8\u6372", "6": "\u5176\u5fe0\u800c\u7fa9\u5b5d\u800c\u516c\u9042\u4e0d\u8fad\u70ba\u4e4b\u6b63\u898b\u5176\u7de8\u4e2d\u6709\u980c\u4e09", "60": "\u5e54\ud848\udd4f\u96a8\u53d6\u4fbf\u89e3\u958b\u5e03\u888b\u5408\u7370\u9f8d\u622a\u65b7\u5343\u5dee\u7121\u7570\u8def\u62ac", "61": "\u982d\u842c\u91cc\u6709\u540c\u98a8\u8af3\u6642\u8d70\u904d\u9084\u4f86\u6b64\u518d\u9020\u967d\u6625\u8f14\u5316\u5d07", "62": "\u5143\u5bb5\u5f8c\u503c\u96ea\u4ea8\u5982\u842c\u516c\u70ba\u5b50\u812b\u767d\u8acb\u4e0a\u5802\u6628\u591c\u5c71\u982d", "63": "\u5806\u96ea\u4eca\u671d\u9280\u6a39\u73b2\u5782\u6a13\u4e0a\u6d2a\u9418\u5927\u6263\u5802\u4e2d\u64be\u9f13\u5982\u96f7", "64": "\u62c2\u5b50\u500f\u7136\ud860\udc5d\u8df3\u62c4\u6756\u5165\u624b\u63da\u5a01\u96d6\u5171\u4e00\u6642\u6a5f\u767c\u6191\u4ed6", "65": "\u767e\u4e16\u5149\u8f1d\u795e\u901a\u5999\u7528\u4e0d\u662f\u683c\u5916\u63d0\u6301\u4ea6\u975e\u4ea8\u5982\u98ef\u50e7", "66": "\u8a2d\u4f9b\u8001\u50e7\u7279\u70ba\u555f\u8ff7\u559d\u4e00\u559d\u4e91\u5fc3\u751f\u5927\u6b61\u559c\u81ea\u77e5\u7576", "67": "\u4f5c\u4f5b\u53c8\u53ec\u609f\u8056\u4e91\u537b\u5e3d\u812b\u978b\u8fd4\u672c\u5b9c\u8d64\u8eab\u7121\u7919\u4ea6\u7121", "68": "\u7d72\u5f9e\u4eca\u653e\u51fa\u4e09\u9580\u53bb\u9060\u7e8c\u4eba\u9593\u7b2c\u4e00\u679d\u3002", "69": "\u4e0a\u5802\u77bf\u66c7\u4eca\u65e5\u793a\u73fe\u4e09\u5cf0\u53d7\u8acb\u4e0a\u5802\u96d6\u662f\u5bb6\u7121\u4e8c\u4e3b", "7": "\u95dc\u8a9e\u672c\u8272\u5feb\u66a2\u53ef\u9304\u9042\u524d\u5f8c\u5208\u8832\u50c5\u5b58\u5176\u53ef\u50b3\u8005\u4e00", "70": "\u8981\u4e14\u540c\u689d\u5171\u8cab\u9a40\u4ee5\u62c4\u6756\u4f5c\u6b64\udb94\udee1\u53ec\u773e\u4e91\u8005\u662f\u6211\u91cb", "71": "\u8fe6\u8001\u4eba\u521d\u964d\u738b\u5bae\u5468\u884c\u4e03\u6b65\u76ee\u9867\u56db\u65b9\u8a87\u7d93\u8ce3\u7d00\u65c1", "72": "\u82e5\u7121\u4eba\u6240\u4ee5\u906d\u4eba\u64bf\u9ede\u81ea\u8a0e\u5176\u8fb1\u4eca\u65e5\u5149\u4ea8\u8acb\u8001\u50e7", "73": "\u767b\u5ea7\u79ea\u5f97\u4f9d\u6a23\u88c1\u596a\u8cb7\u5e3d\u76f8\u982d\u8207\u5c45\u58eb\u5b89\u540d\u53d6\u8af1\u5149", "74": "\u8f1d\u4f5b\u65e5\u559c\u7f8e\u540c\u6d41\u4e14\u9053\u6587\u4e0d\u52a0\u9ede\u4f5c\u9ebc\u751f\u6176\u8cc0\u7fe0\u79c0", "75": "\u5343\u682a\u67cf\u541f\u98a8\u4e00\u6a23\u677e\u3002", "76": "\u80e4\u9d6c\u694a\u516c\u70ba\u7236\u6bcd\u958b\u5960\u8acb\u4e0a\u5802\u7210\u7159\u4e45\u51b7\u672a\u582a\u56b4\u5254", "77": "\u8d77\u7709\u6bdb\u53c8\u5fa9\u7136\u4eca\u65e5\u70ba\u62c8\u9999\u4e00\u74e3\u5168\u4ec1\u5168\u5b5d\u88d4\u7dbf\u7dbf", "78": "\u9673\u5c45\u58eb\u8acb\u4e0a\u5802\u6628\u591c\u77bf\u66c7\u73fe\u745e\u661f\u4eca\u671d\u5ea7\u4e0a\u5076\u50b3\u5fc3", "79": "\u773c\u89aa\u4f55\u4f3c\u624b\u89aa\u597d\u9054\u8005\u61c9\u77e5\u7121\u4e8c\u8eab\u6240\u8b02\u91cb\u8fe6\u898b\u91cf", "8": "\u5341\u4e8c\u7bc7\u5e76\u70ba\u4e4b\u5e8f\u3002", "80": "\u4e0d\u540c\u4e09\u5cf0\u7528\u5ea6\u5404\u5225\u4e14\u9053\u4e09\u5cf0\u8d8a\u683c\u624b\u773c\u5728\u751a\u9ebc\u8655", "81": "\u6562\u723e\u81ea\u8a31\u4e03\u5341\u9918\u7fc1\u4e4d\u5165\u5834\u4f11\u5c07\u8005\u4e8b\u62b5\u9580\u58bb\u5c31\u4e2d", "82": "\u82e5\u8981\u6df1\u59d4\u6089\u53ea\u8cb4\u9592\u5fd9\u81ea\u4e3b\u5f35\u3002", "83": "\u96f2\u6d77\u7f85\u516c\u85a6\u56b4\u8acb\u4e0a\u5802\u5929\u5f97\u4e00\u4ee5\u6e05\u5730\u5f97\u4e00\u4ee5\u5be7\u60df", "84": "\u6709\u4e09\u5cf0\u7121\u53ef\u5f97\u76f4\u62c8\u62c4\u6756\u85a6\u5f80\u751f\u5927\u773e\u9084\u898b\u9ebc\u5353\u62c4", "85": "\u6756\u4e09\u4e0b\u4e91\u516d\u6839\u7e94\u52d5\u88ab\u96f2\u906e\u4e00\u5ff5\u4e0d\u751f\u5168\u9ad4\u73fe\u3002", "86": "\u4f5b\u8a95\u5f35\u5c45\u58eb\u8acb\u4e0a\u5802\u4e00\u8072\u361e\u5730\u8a31\u8ab0\u77e5\u4e03\u6b65\u5468\u884c\u81ea", "87": "\u5f37\u99b3\u6307\u5929\u6307\u5730\u65bd\u5de7\u7385\u5168\u6536\u5168\u653e\u7538\u90aa\u757f\u3002", "88": "\u70ba\u7f85\u5b5d\u5ec9\u5c01\u9748\u5c0f\u53c3\u7576\u5e74\u85a6\u59a3\u8acb\u965e\u5802\u6127\u81f4\u4e8e\u4eca\u5169", "89": "\u9b22\u971c\u89bf\u9762\u4ef0\u89c0\u64be\u6bd2\u9f13\u96f2\u4e2d\u9435\u7b1b\u5c0d\u8ab0\u63da\u3002", "9": "\u6642", "90": "\u4f4f\u8208\u96f2\u79aa\u9662\u5149\u7950\u8acb\u4e0a\u5802\u4e16\u5c0a\u7779\u661f\u609f\u9053\u5225\u5cf0\u64be\u9f13", "91": "\u965e\u5802\u660e\u4eba\u4e0d\u4f5c\u6697\u4e8b\u90fd\u76e7\u4e00\u68d2\u8ab0\u7576\u6709\u4e0d\u9867\u5371\u4ea1\u8005", "92": "\u51fa\u4f86\u8207\u8208\u96f2\u76f8\u898b\u826f\u4e45\u7121\u51fa\u8005\u5e2b\u4e91\u5225\u5cf0\u81ea\u5df1\u627f\u7576", "93": "\u53bb\u4e5f\u4f5c\u75db\u8072\u4e0b\u5ea7\u3002", "94": "\u5c0f\u53c3\u5927\u773e\u5165\u5802\u4e09\u65e5\u500b\u500b\u543b\u5634\u9762\u58c1\u79ea\u9808\u9802\u9580\u5177\u773c", "95": "\u5fc5\u8981\u8173\u8ddf\u9ede\u5730\u4e09\u5cf0\u8005\u88e1\u7121\u79aa\u53ef\u53c3\u7121\u6212\u53ef\u6301\u5373\u4eca", "96": "\u6cd5\u4ee4\u7576\u884c\u8a66\u554f\u8af8\u516c\u9053\u5f97\u4e5f\u8207\u4e09\u5341\u9053\u4e0d\u5f97\u4e5f\u8207\u4e09", "97": "\u5341\u8a66\u9053\u770b\u6578\u50e7\u4e0b\u8a9e\u7686\u4e0d\u5951\u7dad\u90a3\u51fa\u5e2b\u4fbf\u6253\u90a3\u4e91\u6851", "98": "\u6a39\u4e0a\u8457\u7bad\u67f3\u6a39\u4e0a\u51fa\u6f3f\u5e2b\u53c8\u6253\u3002", "99": "\u9664\u591c\u5c0f\u53c3\u4eba\u9593\u4e16\u4e8b\u5fd9\u5982\u7bad\u7368\u6211\u5c71\u4e2d\u7121\u6240\u70ba\u6628\u591c"}, "originalTitle": "Jiaxing Canon, Electronic version, No. B420 \u4e09\u5cf0\u534a\u6c34\u5143\u79aa\u5e2b\u8a9e\u9304", "englishTitle": "Jiaxing Canon, Electronic version, No. B420 \u4e09\u5cf0\u534a\u6c34\u5143\u79aa\u5e2b\u8a9e\u9304", "author": "\u6e05 \u5143\u8aaa\u3000\u5149\u6df1\u7b49\u9304\u3000\u83ef\u56b4\u7389\u5e8f\uff08\u4f9d\u99d2\u672c\u5370\uff09", "language": "chinese", "source": "CBETA"}