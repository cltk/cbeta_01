5": "\u8b9a\u66f0\uff1a", "246": "\u516b\u5341\u96a8\u5f62\u597d\uff0c\u76f8\u5206\u4e09\u5341\u4e8c\u3002", "247": "\u61c9\u5831\u842c\u822c\u8eab\uff0c\u7406\u4e2d\u975e\u4e00\u7570\u3002", "248": "\u4eba\u6cd5\u5169\u4ff1\u9063\uff0c\u8272\u8eab\u9f4a\u4e00\u68c4\u3002", "249": "\u6240\u4ee5\u8b49\u83e9\u63d0\uff0c\u5be6\u7531\u8af8\u76f8\u96e2\u3002", "25": "\u5be6\u8a9e\u8005\u3002\u8aaa\u4e00\u5207\u6cd5\u7121\u6240\u6709\uff0c\u5373\u662f\u5be6\u8a9e\u3002", "250": "\u82e5\u4eba\u8a00\u5982\u4f86\u6709\u8aaa\u6cd5\uff0c\u5373\u70ba\u8b17\u4f5b\u3002\u91cb\u66f0\uff1a\u4f5b\u8eab\u8005", "251": "\u5373\u6cd5\u8eab\u4e5f\uff0c\u6cd5\u8eab\u4f5c\u4f55\u5f62\u76f8\uff0c\u800c\u8a00\u6709\u8aaa\uff0c\u6545\u77e5\u5982\u4f86\u7121", "252": "\u6240\u8aaa\u3002", "253": "\u7121\u6cd5\u53ef\u8aaa\u662f\u540d\u8aaa\u6cd5\u3002\u91cb\u66f0\uff1a\u82e5\u5c0d\u4e94\u9670\u8272\u8eab\u5373\u6709", "254": "\u6240\u8aaa\uff0c\u82e5\u4e86\u8272\u662f\u7a7a\uff0c\u7a7a\u4e2d\u7121\u8aaa\u3002\u82e5\u5c0d\u6cd5\u8eab\uff0c\u6cd5\u8eab\u5177", "255": "\u5408\u842c\u6cd5\uff0c\u662f\u540d\u8aaa\u6cd5\u3002\u66f4\u6709\u4e00\u91cb\uff0c\u7d04\u4e16\u8ae6\u800c\u8a00\uff0c\u70ba\u8272", "256": "\u8eab\u662f\u5984\uff0c\u8aaa\u4ea6\u662f\u5984\uff0c\u7a7a\u6709\u540d\u6545\u4e91\u662f\u8aaa\u6cd5\u3002\u66f4\u6709\u4e00", "257": "\u91cb\uff0c\u82e5\u7d04\u5316\u5883\uff0c\u5373\u662f\u5316\u8eab\u8aaa\u6cd5\u3002\u82e5\u7d04\u7406\u800c\u8a00\uff0c\u5373\u662f", "258": "\u6cd5\u8eab\u8aaa\u6cd5\u3002\u6cd5\u4f5b\u8aaa\u6cd5\uff0c\u60df\u4f5b\u8207\u4f5b\u4e43\u80fd\u77e5\u4e4b\u3002\u975e\u8af8\u4e8c", "259": "\u4e58\u51e1\u592b\u4e4b\u6240\u80fd\u89e3\uff0c\u5df2\u4e0b\u53ef\u77e5\u3002", "26": "\u5982\u8a9e\u8005\u3002\u8aaa\u4e00\u5207\u8af8\u6cd5\uff0c\u672c\u4f86\u4e0d\u52d5\uff0c\u6709\u4f55\u4e0d\u5982\uff1f", "260": "\u662f\u6cd5\u5e73\u7b49\uff0c\u7121\u6709\u9ad8\u4e0b\u3002\u4e8c\u4e58\u4e4b\u4eba\u70ba\u4f5b\u662f\u9ad8\uff0c", "261": "\u773e\u751f\u662f\u4e0b\uff0c\u6b64\u662f\u5984\u8b58\u5206\u5225\uff0c\u4f9d\u8b58\u800c\u89e3\uff0c\u83e9\u85a9\u4e86\u4eba\u6cd5", "262": "\u7a7a\uff0c\u672c\u4f86\u7121\u5984\u8b58\uff0c\u7121\u6240\u5206\u5225\uff0c\u5373\u4e00\u5207\u6cd5\u5e73\u7b49\uff0c\u7121\u6709", "263": "\u9ad8\u4e0b\u3002\u4e8c\u4e58\u7121\u76ee\uff0c\u4e0d\u898b\u81ea\u5fc3\uff0c\u81ea\u751f\u9ad8\u4e0b\u60f3\uff0c\u70ba\u6d85\u69c3", "264": "\u662f\u9ad8\uff0c\u751f\u6b7b\u70ba\u4e0b\uff0c\u83e9\u63d0\u662f\u9ad8\uff0c\u7169\u60f1\u662f\u4e0b\u3002\u4e00\u5207\u842c", "265": "\u6cd5\uff0c\u5be6\u7121\u9ad8\u4e0b\uff0c\u4eba\u5fc3\u81ea\u6709\u9ad8\u4e0b\u76f8\uff0c\u662f\u6545\u87ba\u9afb\u68b5\u738b", "266": "\u8a00\uff1a\u4eba\u8005\u5fc3\u6709\u9ad8\u4e0b\uff0c\u4e0d\u4f9d\u4f5b\u60e0\u3002\u4e8c\u4e58\u4e4b\u4eba\uff0c\u4ea6\u5fa9\u5982", "267": "\u662f\uff0c\u5fc3\u6709\u9ad8\u4e0b\uff0c\u4e0d\u4f9d\u6b63\u6cd5\uff0c\u6545\u898b\u6709\u9ad8\u4e0b\u3002", "268": "\u8b9a\u66f0\uff1a", "269": "\u6c34\u9678\u540c\u771f\u969b\uff0c\u98db\u884c\u9ad4\u4e00\u5982\u3002", "27": "\u4e0d\u8a91\u8a9e\u8005\u3002\u4e00\u5207\u773e\u751f\u805e\u5982\u662f\u8aaa\uff0c\u7686\u5f97\u89e3\u812b\uff0c\u4e91", "270": "\u7406\u4e2d\u4f55\u5f7c\u6b64\uff0c\u6cd5\u4e0a\u8c48\u89aa\u758f\u3002", "271": "\u81ea\u4ed6\u5206\u5225\u9063\uff0c\u9ad8\u4e0b\u57f7\u60c5\u9664\u3002", "272": "\u4e86\u77e5\u5e73\u7b49\u6027\uff0c\u54b8\u5171\u5165\u7121\u9918\u3002", "273": "\u4fee\u4e00\u5207\u5584\u6cd5\u5373\u5f97\u83e9\u63d0\u3002\u5373\u662f\u822c\u82e5\u6ce2\u7f85\u871c\u6cd5\uff0c\u5373", "274": "\u662f\u83e9\u63d0\u3002\u4e0b\u6587\u4e91\uff1a", "275": "\u6240\u8a00\u5584\u6cd5\u8005\uff0c\u5982\u4f86\u8aaa\u975e\u5584\u6cd5\u3002\u91cb\u66f0\uff1a\u975e\u8005\u4e91", "276": "\u7a7a\u4e86\u4eba\u6cd5\u4e8c\u7a7a\uff0c\u5373\u7121\u60e1\u7121\u5584\uff0c\u65e2\u4e0d\u4f5c\u60e1\uff0c\u5373\u7121\u5730", "277": "\u7344\u3002\u82e5\u4e0d\u4fee\u5584\uff0c\u5373\u7121\u5929\u5802\u3002\u80fd\u5982\u662f\u77e5\uff0c\u5373\u4e0d\u88ab\u4e8c\u5883", "278": "\u6240\u651d\uff0c\u5373\u7121\u6240\u62d8\uff0c\u5373\u662f\u83e9\u63d0\u4e4b\u8def\u3002\u6545\u4e0b\u6587\u4e91\uff1a\u662f\u540d", "279": "\u5584\u6cd5\u3002\u6b21\u4e0b\u8f03\u91cf\u66f4\u7121\u5225\u7fa9\u3002", "28": "\u4f55\u5f97\u8a91\uff1f", "280": "\u5be6\u7121\u773e\u751f\u5982\u4f86\u5ea6\u8005\u3002\u91cb\u66f0\uff1a\u773e\u751f\u8005\uff0c\u7957\u662f\u6c5d", "281": "\u5fc3\uff0c\u5984\u60f3\u5206\u5225\uff1b\u7a2e\u7a2e\u985b\u5012\uff0c\u5373\u662f\u773e\u751f\uff1b\u4f5b\u51fa\u65bc\u4e16\uff0c", "282": "\u7957\u6c5d\u5984\u5fc3\u3002\u601d\u76ca\u7d93\u4e91\uff1a\u4f5b\u51fa\u65bc\u4e16\uff0c\u70ba\u5ea6\u5984\u60f3\u3002\u723e", "283": "\u4e0d\u4e86\u4eba\u6cd5\u4e8c\u7a7a\uff0c\u70ba\u5b9a\u6709\u5982\u662f\u7169\u60f1\u773e\u751f\u53ef\u5ea6\u3002\u82e5\u4e86\u4eba", "284": "\u6cd5\u4e8c\u7a7a\uff0c\u5373\u7121\u5984\u60f3\u773e\u751f\u53ef\u5ea6\u3002\u5982\u662f\u5ea6\u8005\uff0c\u5373\u540d\u771f", "285": "\u5ea6\u3002\u82e5\u5ea6\u5916\u773e\u751f\uff0c\u5373\u5408\u7e3d\u5ea6\u76e1\uff0c\u4e91\u4f55\u5373\u4eca\u6709\u5982\u8a31\u773e", "286": "\u751f\u53d7\u6d41\u6d6a\u751f\u6b7b\uff1f\u6545\u7d93\u4e91\uff1a\u8ff7\u5373\u773e\u751f\uff0c\u609f\u5373\u662f\u4f5b\u3002\u4f5b", "287": "\u51fa\u65bc\u4e16\uff0c\u6559\u793a\u4eba\uff1a\u81ea\u88fd\u516d\u6839\uff0c\u964d\u65b7\u4e09\u6bd2\uff0c\u820d\u8caa\u55d4", "288": "\u7661\u3002\u51e1\u592b\u7528\u4f5b\u8a9e\uff0c\u5982\u6b64\u7b49\u4e8b\u6f38\u6f38\u8f15\u5fae\uff0c\u5373\u662f\u5ea6\u773e", "289": "\u751f\uff0c\u82e5\u8b49\u771f\u7a7a\u4e4b\u7406\uff0c\u5be6\u7121\u53ef\u5ea6\u3002\u4e0b\u6587\u4e91\uff1a", "29": "\u4e0d\u7570\u8a9e\u8005\u3002\u6709\u662f\u8aaa\u6709\uff0c\u7121\u662f\u8aaa\u7121\uff0c\u771f\u662f\u8aaa\u771f\uff0c", "290": "\u82e5\u6709\u773e\u751f\u5982\u4f86\u5ea6\u8005\uff0c\u5982\u4f86\u5373\u6709\u6211\u3001\u4eba\u3001\u773e\u751f\u3001", "291": "\u58fd\u8005\u3002\u91cb\u66f0\uff1a\u5982\u4f86\u82e5\u6709\u5f62\u76f8\uff0c\u5373\u53ef\u8a00\uff1a\u5982\u4f86\u5ea6\u773e", "292": "\u751f\u3002\u5982\u4f86\u65e2\u7121\u76f8\u8c8c\uff0c\u8ab0\u898b\u5ea6\u773e\u751f\uff1f\u773e\u751f\u7121\u5f62\u76f8\uff0c", "293": "\u4f5b\u4ea6\u7121\u5f62\uff0c\u4e8c\u4ff1\u7121\u76f8\uff0c\u4e0d\u53ef\u4ee5\u7121\u76f8\u5ea6\u7121\u76f8\u3002\u4e0b\u6587", "294": "\u4e91\uff1a", "295": "\u5982\u4f86\u8aaa\u6709\u6211\u8005\uff0c\u5247\u975e\u6709\u6211\uff0c\u800c\u51e1\u592b\u4e4b\u4eba\u4ee5\u70ba", "296": "\u6709\u6211\u3002\u91cb\u66f0\uff1a\u4e8c\u4e58\u3001\u51e1\u592b\uff0c\u57f7\u6709\u6211\u76f8\uff0c\u6240\u4ee5\u96e2\u751f\u6b7b", "297": "\u800c\u6c42\u6d85\u69c3\uff0c\u820d\u7169\u60f1\u800c\u6c42\u83e9\u63d0\uff0c\u820d\u4e00\u908a\uff0c\u53d6\u4e00\u908a\uff0c\u6545", "298": "\u540d\u51e1\u592b\u3002\u4e0b\u6587\u4e91\uff1a", "299": "\u51e1\u592b\u8005\uff0c\u5982\u4f86\u8aaa\u5247\u975e\u51e1\u592b\u3002\u91cb\u66f0\uff1a\u51e1\u592b\u4ea6\u7a7a\uff0c", "3": "\u4e00\u65e5\u53ef\u9054\uff0c\u5728\u8def\u25a1\u25a1\u7a7a\u820d\u800c\u884c\u61c8\u6020\uff0c\u6b32\u751f\u5176\u4e2d\uff0c\u6b64\u5c6c", "30": "\u5047\u662f\u8aaa\u5047\uff0c\u4f55\u5f97\u6709\u7570\uff1f\u53c8\u4e0d\u7570\u8a9e\u8005\uff0c\u4e00\u5207\u842c\u6cd5\u672c\u6027", "300": "\u8ff7\u8005\u5984\u57f7\u3002\u4f46\u7121\u57f7\u5fc3\uff0c\u4e00\u5207\u6e05\u6de8\u3002\u8f49\u8f2a\u8056\u738b\u4e43\u662f\u897f", "301": "\u570b\u5929\u5b50\uff0c\u4ea6\u6709\u4e09\u5341\u4e8c\u76f8\uff0c\u4f5b\u4ea6\u6709\u4e09\u5341\u4e8c\u76f8\uff0c\u5982\u4f55\u5206", "302": "\u5225\uff1f\u4f5b\u6050\u773e\u751f\u4e0d\u80fd\u5206\u5225\uff0c\u6545\u4f5c\u662f\u554f\u3002\u7a7a\u751f\u89e3\u7a7a\u7b2c", "303": "\u4e00\uff0c\u4f5b\u6240\u4ee5\u5c07\u5982\u4f86\u540d\u5b57\u76f8\uff0c\u6216\u6545\u554f\u6c5d\uff1a\u4ee5\u4e09\u5341\u4e8c\u76f8", "304": "\u89c0\u5982\u4f86\u4e0d\uff1f\u9808\u83e9\u63d0\u679c\u7136\u57f7\u76f8\u800c\u7b54\uff1a", "305": "\u5982\u662f\u5982\u662f\uff0c\u4ee5\u4e09\u5341\u4e8c\u76f8\u89c0\u5982\u4f86\u3002\u4f5b\u5373\u554f\u9053\uff1a", "306": "\u82e5\u4ee5\u4e09\u5341\u4e8c\u76f8\u89c0\u5982\u4f86\u8005\uff0c\u8f49\u8f2a\u8056\u738b\u5247\u662f\u5982", "307": "\u4f86\u3002\u7a7a\u751f\u5ffd\u609f\uff1a", "308": "\u5982\u6211\u89e3\u4f5b\u6240\u8aaa\u7fa9\uff0c\u4e0d\u61c9\u4ee5\u4e09\u5341\u4e8c\u76f8\u89c0\u5982\u4f86\u3002\u91cb", "309": "\u66f0\uff1a\u4e09\u5341\u4e8c\u76f8\u5373\u662f\u8272\u8eab\uff0c\u4ea6\u662f\u76f8\u8c8c\uff1b\u5982\u4f86\u7121\u8272\uff0c\u4ea6", "31": "\u81ea\u7a7a\uff0c\u5c07\u4f55\u70ba\u7570\uff1f\u4e0b\u6587\u4e91\u662f\u6cd5\u5e73\u7b49\uff0c\u4f55\u5f97\u6709\u7570\uff1f", "310": "\u7121\u76f8\u8c8c\uff0c\u4e0d\u53ef\u5c07\u76f8\u8c8c\u4ee5\u70ba\u5982\u4f86\uff0c\u4ea6\u4e0d\u53ef\u5c07\u97f3\u8072\u4ee5\u70ba", "311": "\u5982\u4f86\uff0c\u82e5\u4ee5\u97f3\u8072\u70ba\u5982\u4f86\u8005\uff0c\u98a8\u8072\u3001\u6c34\u8072\u3001\u9ce5\u8072\u3001\u7378", "312": "\u8072\uff0c\u5247\u5408\u662f\u5982\u4f86\u3002\u5982\u4f86\u7121\u8272\u7121\u8072\uff0c\u662f\u6545\u5048\u4e91\uff1a", "313": "\u82e5\u4ee5\u8272\u898b\u6211\uff0c\u4ee5\u97f3\u8072\u6c42\u6211\u3002\u662f\u4eba\u884c\u90aa\u9053\uff0c\u4e0d\u80fd", "314": "\u898b\u5982\u4f86\u3002\u91cb\u66f0\uff1a\u6211\u8005\u662f\u81ea\u6027\u4e4b\u7fa9\uff0c\u662f\u540d\u771f\u6211\u3002\u97f3\u8005", "315": "\u662f\u9748\u4e4b\u5225\u540d\uff0c\u4ea6\u540d\u795e\u9748\u3002\u96e2\u6b64\u800c\u6c42\uff0c\u7686\u843d\u90aa\u9053\u3002\u8272", "316": "\u4ea6\u662f\u82d7\uff0c\u8072\u4ea6\u662f\u82d7\u3002\u672b\u6cd5\u6642\u4e2d\uff0c\u6240\u6709\u4fee\u5b78\u8005\uff0c\u7686\u8a8d", "317": "\u76f8\u53ca\u8a8d\u97f3\u8072\u8005\uff0c\u591a\u5982\u662f\u4e4b\u4eba\uff0c\u76e1\u4e0d\u5951\u771f\u7406\u3002", "318": "\u8b9a\u66f0\uff1a", "319": "\u6d85\u69c3\u542b\u56db\u5fb7\uff0c\u552f\u6211\u5951\u771f\u5e38\u3002", "32": "\u8b9a\u66f0\uff1a", "320": "\u97f3\u540d\u516b\u81ea\u5728\uff0c\u7368\u4e00\u6700\u9748\u9577\u3002", "321": "\u975e\u8272\u975e\u8072\u76f8\uff0c\u5fc3\u8b58\u8c48\u80fd\u91cf\u3002", "322": "\u770b\u6642\u4e0d\u53ef\u898b\uff0c\u609f\u7406\u5373\u5f62\u5f70\u3002", "323": "\u83ab\u4f5c\u662f\u5ff5\u3002\u4f5b\u6050\u7a7a\u751f\u843d\u65b7\u6ec5\u898b\uff0c\u6545\u793a\u5408\u96e2\u5169", "324": "\u908a\u3002\u524d\u65e2\u4e91\uff1a\u89c0\u5982\u4f86\u4e0d\u5f97\u4ee5\u5177\u8db3\u76f8\u89c0\u3002\u4eca\u53c8\u6050\u7a7a\u751f", "325": "\u57f7\u8457\u4e0d\u4f5c\u5177\u8db3\u76f8\uff0c\u6545\u9053\uff1a\u83ab\u4f5c\u662f\u5ff5\uff0c\u5982\u4f86\u4e0d\u4ee5\u5177\u8db3", "326": "\u76f8\u6545\uff0c\u5f97\u963f\u8028\u83e9\u63d0\u3002\u53c8\u5411\u7a7a\u751f\u9053\uff1a", "327": "\u5982\u82e5\u4f5c\u662f\u5ff5\u767c\u83e9\u63d0\u8005\uff0c\u8aaa\u8af8\u6cd5\u65b7\u6ec5\u3002\u91cb\u66f0\uff1a\u82e5", "328": "\u4f5c\u6709\u76f8\u89c0\uff0c\u5373\u662f\u4e00\u908a\u898b\uff1b\u82e5\u4f5c\u7121\u76f8\u89c0\uff0c\u4ea6\u662f\u4e00\u908a", "329": "\u898b\uff1b\u82e5\u4f5c\u6709\u7121\u89c0\uff0c\u5373\u662f\u65b7\u6ec5\u6cd5\u3002\u4e0b\u6587\u4e91\uff1a", "33": "\u773e\u751f\u8207\u860a\u754c\uff0c\u540d\u5225\u9ad4\u975e\u6b8a\u3002", "330": "\u65bc\u6cd5\u4e0d\u8aaa\u65b7\u6ec5\u76f8\u3002\u91cb\u66f0\uff1a\u771f\u5982\u6cd5\u6027\uff0c\u975e\u662f\u6709", "331": "\u7121\uff1b\u771f\u5982\u4e4b\u5fc3\uff0c\u6e5b\u7136\u4e0d\u52d5\u3002\u89c0\u8207\u4e0d\u89c0\uff0c\u7686\u662f\u8d77\u4f5c\uff1b", "332": "\u8d77\u4f5c\u4e4b\u5fc3\uff0c\u5143\u662f\u751f\u6ec5\uff0c\u6545\u8a00\u83ab\u4f5c\u662f\u5ff5\u3002", "333": "\u82e5\u5fa9\u6709\u4eba\uff0c\u77e5\u4e00\u5207\u6cd5\u7121\u6211\uff0c\u5f97\u6210\u65bc\u5fcd\u3002\u91cb\u66f0\uff1a", "334": "\u4e00\u5207\u842c\u6cd5\uff0c\u672c\u4f86\u4e0d\u751f\uff0c\u8ab0\u8a00\u6709\u6211\u3001\u7121\u6211\uff1f\u5982\u662f\u898b", "335": "\u8005\uff0c\u5373\u540d\u70ba\u5fcd\uff0c\u5fcd\u5373\u7121\u751f\u5fcd\u3002", "336": "\u6b64\u83e9\u85a9\u52dd\u524d\u83e9\u85a9\u6240\u5f97\u529f\u5fb7\u3002\u91cb\u66f0\uff1a\u5982\u662f\u898b\u8005\uff0c", "337": "\u52dd\u4e03\u5bf6\u5e03\u65bd\u529f\u5fb7\u3002", "338": "\u8b9a\u66f0\uff1a", "339": "\u65bd\u5bf6\u5982\u6c99\u6578\uff0c\u552f\u6210\u6709\u6f0f\u56e0\u3002", "34": "\u4e86\u5fc3\u5982\u4f3c\u5e7b\uff0c\u8ff7\u60c5\u898b\u6709\u9918\u3002", "340": "\u4e0d\u5982\u7121\u6211\u89c0\uff0c\u4e86\u5984\u4e43\u540d\u771f\u3002", "341": "\u6b32\u8b49\u7121\u751f\u5fcd\uff0c\u8981\u5047\u96e2\u60c5\u5875\u3002", "342": "\u5e38\u4f9d\u4e8c\u7a7a\u7406\uff0c\u7576\u8b49\u6cd5\u738b\u8eab\u3002", "343": "\u4e0d\u53d7\u798f\u5fb7\u3002\u898b\u8eab\u76f8\u662f\u6709\uff0c\u5373\u53d7\u798f\u5fb7\u3002\u8eab\u5c1a\u81ea", "344": "\u7121\uff0c\u6559\u8ab0\u53d7\u798f\u5fb7\uff1f\u6b21\u660e\u6cd5\u8eab\uff0c\u4ea6\u8faf\u5982\u4f86\u4e4b\u7fa9\u3002\u4e16\u9593", "345": "\u4e4b\u4eba\uff0c\u7686\u4e91\uff1a\u5982\u4f86\u884c\u4f4f\u5750\u81e5\uff0c\u6b64\u662f\u90aa\u898b\u3002\u884c\u4f4f\u5750\u81e5", "346": "\u8005\uff0c\u975e\u662f\u5982\u4f86\uff0c\u5982\u4f86\u7121\u5f62\u7121\u76f8\uff0c\u4e0d\u53ef\u8a00\u884c\u4f4f\u5750\u81e5\u662f", "347": "\u5982\u4f86\u3002\u5982\u4f86\u8005\uff0c\u4e0d\u4f86\u4e0d\u53bb\uff0c\u5373\u662f\u5982\u4f86\u3002\u6545\u7d93\u4e91\uff1a", "348": "\u7121\u6240\u5f9e\u4f86\uff0c\u4ea6\u7121\u6240\u53bb\uff0c\u6545\u540d\u5982\u4f86\u3002\u91cb\u66f0\uff1a\u4f5b\u6027", "349": "\u7121\u751f\uff0c\u6545\u540d\u7121\u4f86\uff1b\u81ea\u6027\u4e0d\u6ec5\uff0c\u6545\u4e91\u7121\u53bb\uff1b\u4e0d\u751f\u4e0d", "35": "\u771f\u8a00\u8a00\u4e0d\u5984\uff0c\u5be6\u8a9e\u8a9e\u975e\u865b\u3002", "350": "\u6ec5\uff0c\u5373\u662f\u5982\u4f86\u3002\u82e5\u8a00\u884c\u4f4f\u5750\u81e5\u8005\u662f\u5982\u4f86\uff0c\u5373\u5c6c\u751f", "351": "\u6ec5\uff0c\u4e91\u4f55\u540d\u4f5b\uff1f", "352": "\u5fae\u5875\u8005\u3002\u5373\u662f\u5984\u5fc3\u4e4b\u7570\u540d\u3002", "353": "\u4e16\u754c\u8005\uff0c\u5373\u8eab\u4e4b\u5225\u5b57\uff0c\u6b64\u8eab\u4e43\u662f\u5984\u5fc3\u4e4b\u6240\u6210", "354": "\u7acb\u3002\u82e5\u8eab\u662f\u6709\uff0c\u4e43\u81f3\u5fc3\u6709\u8005\uff0c\u5247\u662f\u4e00\u5408\u76f8\uff0c\u8eab\u5fc3\u4ff1", "355": "\u7121\uff0c\u5c07\u4f55\u70ba\u4e00\u5408\u76f8\uff1f\u6545\u4e91\uff1a", "356": "\u5247\u975e\u4e00\u5408\u76f8\u3002\u4e0b\u6587\u4e91\uff1a", "357": "\u4e00\u5408\u76f8\u8005\uff0c\u5247\u4e0d\u53ef\u8aaa\u3002\u91cb\u66f0\uff1a\u592b\u8a00\u4e00\u5408\u8005\uff0c\u7121", "358": "\u4e00\u53ef\u4e00\uff0c\u7121\u5408\u53ef\u5408\uff0c\u842c\u6cd5\u7121\u540d\uff0c\u66f4\u6709\u4f55\u53ef\u8aaa\uff1f\u4e0b\u6587", "359": "\u4e91\uff1a", "36": "\u59cb\u7d42\u7121\u8b8a\u7570\uff0c\u4f5b\u6027\u672c\u4f86\u5982\u3002", "360": "\u4f46\u51e1\u592b\u4e4b\u4eba\uff0c\u8caa\u8457\u5176\u4e8b\u3002\u91cb\u66f0\uff1a\u82e5\u898b\u6709\u4e00\u4e8b\uff0c", "361": "\u5373\u4e0d\u540d\u4e00\u5408\u76f8\uff0c\u5373\u6709\u5dee\u5225\u3002\u82e5\u6709\u5dee\u5225\uff0c\u5373\u6709\u751f\u6b7b\uff0c", "362": "\u4ee5\u6709\u751f\u6b7b\uff0c\u6545\u865f\u51e1\u592b\u3002\u6b21\u7252\u898b\uff1a\u524d\u4e00\u7252\u662f\u8072\u805e\uff0c\u6b21", "363": "\u4e00\u7252\u662f\u7de3\u89ba\uff0c\u4e0b\u4e00\u7252\u662f\u5927\u4e58\u4fee\u5b78\u4e4b\u4eba\u3002\u61c9\u4f5c\u5982\u662f", "364": "\u77e5\uff0c\u83ab\u53d6\u524d\u4e8c\u7a2e\u898b\uff0c\u6b64\u898b\u8457\u4e8c\u908a\uff0c\u5f8c\u898b\u4e0d\u5c6c\u6709\u7121\u3002", "365": "\u4e0b\u6587\u4e91\uff1a", "366": "\u61c9\u5982\u662f\u77e5\u3001\u5982\u662f\u898b\u3001\u5982\u662f\u4fe1\u89e3\uff0c\u4e0d\u751f\u6cd5\u76f8\u3002", "367": "\u91cb\u66f0\uff1a\u4f46\u4e0d\u4f5c\u6709\u898b\uff0c\u4e0d\u4f5c\u7121\u898b\uff0c\u5373\u4e0d\u4f4f\u6cd5\u76f8\u3002\u4e0b\u6587", "368": "\u4e91\uff1a", "369": "\u6240\u8a00\u6cd5\u76f8\u8005\uff0c\u5982\u4f86\u8aaa\uff1a\u5373\u975e\u6cd5\u76f8\u3002\u91cb\u66f0\uff1a\u4e00\u5207", "37": "\u6b64\u6cd5\u7121\u5be6\u7121\u865b\u3002\u865b\u662f\u4e00\u908a\uff0c\u5be6\u662f\u4e00\u908a\uff0c\u4e8c\u908a\u4ff1", "370": "\u6cd5\u5c1a\u81ea\u7a7a\u7121\uff0c\u4f55\u6cc1\u6709\u76f8\uff1f", "371": "\u662f\u540d\u6cd5\u76f8\u3002\u4e86\u6cd5\u53ca\u76f8\uff0c\u4e8c\u4ff1\u7a7a\u6545\uff0c\u5373\u662f\u6cd5\u76f8\u3002", "372": "\u4e00\u5207\u8af8\u6cd5\uff0c\u672c\u81ea\u5982\u5982\uff0c\u7121\u6709\u52d5\u76f8\u3002\u70ba\u773e\u751f\u8aaa\u5982\u662f", "373": "\u6cd5\uff0c\u5373\u662f\u6b63\u8aaa\u3002", "374": "\u4e0d\u53d6\u65bc\u76f8\u3002\u7121\u76f8\u53ef\u53d6\uff0c\u975e\u662f\u8d77\u5fc3\uff0c\u82e5\u6709\u5fc3\u4e0d", "375": "\u53d6\uff0c\u9084\u662f\u6709\u53d6\u3002\u4e0d\u53d6\u8005\uff0c\u672c\u4f86\u7121\u5fc3\uff0c\u6545\u4e91\u4e0d\u53d6\u65bc", "376": "\u76f8\u3002\u76f8\u4ea6\u662f\u7a7a\uff0c\u5fc3\u4ea6\u662f\u7a7a\uff0c\u7121\u5fc3\u5fa9\u7121\u76f8\uff0c\u6709\u4f55\u53ef", "377": "\u53d6\uff1f\u6545\u4e91\u8033\u3002", "378": "\u4e00\u5207\u6709\u70ba\u6cd5\u3002\u4f46\u662f\u4e16\u9593\u6709\u60c5\uff0c\u9752\u3001\u9ec3\u3001\u8d64\u3001", "379": "\u767d\uff0c\u7686\u662f\u6709\u70ba\uff1b\u8d77\u5fc3\u3001\u4e0d\u8d77\u5fc3\uff0c\u4ea6\u662f\u6709\u70ba\u3002\u82e5\u662f\u4f5c", "38": "\u9063\uff0c\u6545\u8a00\u7121\u5be6\u7121\u865b\u3002\u6b64\u6cd5\u771f\u7a7a\u4e4b\u7406\uff0c\u4e8c\u4e58\u4e4b\u4eba\u6ec5\u8272", "380": "\u76f8\uff0c\u5373\u6709\u70ba\uff1b\u8a00\u6211\u4e0d\u4f5c\u4e0d\u53d6\uff0c\u4ea6\u662f\u6709\u70ba\u3002", "381": "\u5982\u5922\u5e7b\u6ce1\u5f71\u3002\u5373\u4eca\u59bb\u5b50\u3001\u5152\u5973\u3001\u7236\u6bcd\u3001\u59ca\u59b9\u3001", "382": "\u5144\u5f1f\u3001\u8eca\u99ac\u3001\u9322\u8ca1\u3001\u91d1\u9280\u3001\u7460\u7483\uff0c\u4e43\u81f3\u81ea\u8eab\u9ad8\u8cb4\u3001", "383": "\u8070\u660e\u3001\u667a\u60e0\u7686\u662f\u5922\u76f8\u898b\u3002\u8b6c\u5982\u5922\u4e2d\u5f97\u5bcc\u8cb4\uff0c\u7a2e\u7a2e\u5bf6", "384": "\u7269\u3001\u5b85\u820d\uff0c\u53ca\u5176\u89ba\u5f8c\uff0c\u4e00\u7269\u4ea6\u7121\u3002\u5373\u4eca\u73fe\u4e16\uff0c\u4ea6\u5fa9", "385": "\u5982\u662f\uff0c\u8ff7\u6642\u4e09\u754c\u6709\uff0c\u609f\u7f77\u5341\u65b9\u7a7a\u3002\u5982\u4eca\u898b\u6709\u8005\uff0c\u5982", "386": "\u6c34\u4e0a\u6ce1\uff0c\u5982\u7661\u5152\u898b\u6c34\u6ce1\uff0c\u70ba\u8a00\u662f\u5bf6\u73e0\u3002\u73fe\u5728\u8eab\u5373\u662f", "387": "\u5f71\uff0c\u7121\u6709\u771f\u5be6\uff0c\u540d\u4e4b\u70ba\u5f71\u3002\u5e7b\u8005\uff0c\u662f\u4e0d\u5b9a\u4e4b\u610f\u3002", "388": "\u5982\u9732\u4ea6\u5982\u96fb\u3002\u4e00\u5207\u6709\u70ba\u6cd5\uff0c\u5982\u8349\u9802\u9732\uff0c\u9808\u81fe\u5373", "389": "\u6d88\uff1b\u5982\u96f2\u4e2d\u96fb\uff0c\u9808\u81fe\u5373\u6ec5\u3002\u4ee5\u8981\u8a00\u4e4b\uff0c\u7957\u662f\u89c0\u4e94\u860a", "39": "\u6b78\u7a7a\uff0c\u7336\u662f\u751f\u6ec5\uff0c\u4ea6\u540d\u5916\u9053\u7a7a\u3002\u7a7a\u8272\u4ff1\u9063\uff0c\u4e0d\u7acb\u6709", "390": "\u7a7a\uff1a\u89c0\u8272\u5982\u5922\uff0c\u89c0\u53d7\u5982\u6ce1\uff0c\u89c0\u60f3\u5982\u5f71\uff0c\u89c0\u884c\u5982\u9732\uff0c", "391": "\u89c0\u8b58\u5982\u96fb\u3002\u6b64\u4e94\u7a2e\u89c0\uff0c\u4ea6\u662f\u5e7b\u5316\uff0c\u5316\u672a\u6709\u662f\u5be6\u3002\u82e5", "392": "\u70ba\u521d\u5fc3\uff0c\u8981\u7528\u662f\u89c0\uff0c\u76e1\u5176\u89c0\u5df2\uff0c\u7a76\u7adf\u7121\u89c0\u3002\u4e0b\u6587", "393": "\u4e91\uff1a", "394": "\u61c9\u4f5c\u5982\u662f\u89c0\u3002", "395": "\u8b9a\u66f0\uff1a", "396": "\u5982\u661f\u7ff3\u71c8\u5e7b\uff0c\u7686\u70ba\u55bb\u7121\u5e38\u3002", "397": "\u6f0f\u8b58\u4fee\u56e0\u679c\uff0c\u8ab0\u80fd\u5f97\u4e45\u9577\u3002", "398": "\u5371\u8106\u540c\u6ce1\u9732\uff0c\u5982\u96f2\u5f71\u96fb\u5149\u3002", "399": "\u9952\u7d93\u516b\u842c\u52ab\uff0c\u7d42\u7adf\u843d\u7a7a\u4ea1\u3002", "4": "\u7686\u662f\u6016\u754f\u4e4b\u623f\u4e2d\u6709\u60e1\u9b3c\u53ca\u6709\u6bd2\u86c7\uff0c\u884c\u4eba\u82e5\u4f4f\uff0c\u5fc5\u6709", "40": "\u7121\uff0c\u8a9e\u563f\u96d9\u4ea1\uff0c\u662f\u975e\u8569\u76e1\uff0c\u975e\u5fc3\u6240\u6e2c\uff0c\u975e\u53e3\u6240\u5ba3\uff0c", "400": "\u7d93\u66f0\uff1a\u4f5b\u8aaa\u662f\u7d93\u5df2\u3002\u4eca\u5247\u4e0d\u7136\uff0c\u4f5b\u8aaa\u662f\u7a7a\u5df2\u3002", "401": "\u9577\u8001\u9808\u83e9\u63d0\u53ca\u8af8\u6bd4\u4e18\u3001\u6bd4\u4e18\u5c3c\u3002\u91cb\u66f0\uff1a\u9808\u83e9\u63d0", "402": "\u662f\u7a7a\u7fa9\uff0c\u5f9e\u672c\u5df2\u4f86\uff0c\u672a\u66fe\u66ab\u6709\uff0c\u6545\u540d\u9577\u8001\u3002\u6bd4\u4e18", "403": "\u8005\uff0c\u6b64\u540d\u81ea\u6027\u7121\u60e1\u3002\u6bd4\u4e18\u5c3c\u8005\uff0c\u6b64\u540d\u81ea\u6027\u7a7a\u3002\u5982\u662f", "404": "\u4e86\u7fa9\uff0c\u5373\u662f\u8aaa\u7d93\u5df2\u3002", "405": "\u512a\u5a46\u585e\u3001\u512a\u5a46\u5937\u8005\u3002\u6b64\u662f\u68b5\u97f3\uff0c\u6b64\u571f\u540d\u4fe1\u3002\u512a", "406": "\u5a46\u585e\uff0c\u540d\u6e05\u4fe1\uff1b\u512a\u5a46\u5937\u8005\uff0c\u6b64\u540d\u4e45\u4fe1\u3002", "407": "\u4e00\u5207\u4e16\u9593\uff0c\u5929\u3001\u4eba\u3002\u6b64\u540d\u6e05\u6de8\u884c\u3002", "408": "\u963f\u4fee\u7f85\u3002\u4e0d\u5b9a\u5fc3\u3002", "409": "\u805e\u4f5b\u6240\u8aaa\uff0c\u7686\u5927\u6b61\u559c\uff0c\u4fe1\u53d7\u5949\u884c\u3002\u91cb\u66f0\uff1a\u82e5\u7d04", "41": "\u6709\u4e0d\u80fd\u6709\uff0c\u7121\u4e0d\u80fd\u7121\uff0c\u6545\u8a00\u7121\u5be6\u7121\u865b\u3002", "410": "\u4e8b\u76f8\uff0c\u5373\u662f\u8a31\u591a\u807d\u773e\uff0c\u4ea6\u540d\u540c\u805e\u773e\u3002\u4eca\u7d04\u7406\u6703\uff0c\u7fa9", "411": "\u5373\u4e0d\u7136\u3002\u8a31\u591a\u5118\u662f\u884c\u4f4d\uff0c\u672a\u609f\u4e4b\u6642\uff0c\u8a31\u591a\u884c\u4f4d\uff0c\u53ca", "412": "\u5176\u609f\u4e86\uff0c\u76e1\u540c\u6cd5\u8eab\uff0c\u6545\u8a00\u6b61\u559c\u5949\u884c\u3002\u82e5\u7d04\u4e8b\u76f8\u89e3", "413": "\u8005\uff0c\u5982\u8a31\u591a\u4eba\uff0c\u4e0d\u89e3\u6b61\u559c\uff0c\u756a\u6295\u81ea\u6b78\uff0c\u540d\u70ba\u5949\u884c\u3002", "414": "\u53c8\u6709\u4eba\u8a00\uff1a\u4f9d\u6cd5\u4fee\u884c\uff0c\u540d\u70ba\u5949\u884c\u3002\u82e5\u5982\u7406\u8005\uff0c\u5982\u4eca", "415": "\u5373\u4e0d\u5206\uff0c\u66f4\u6709\u5929\u3001\u4eba\u3001\u963f\u4fee\u7f85\u7b49\u3002\u82e5\u4f5c\u76f8\u89e3\uff0c\u7121\u6709", "416": "\u662f\u8655\uff0c\u5982\u4f86\u8a2d\u6559\uff0c\u7686\u7d04\u7576\u8eab\uff0c\u81ea\u5316\u81ea\u5ea6\u3002\u7957\u4e0d\u80fd\u81ea", "417": "\u609f\uff0c\u8981\u9808\u5927\u5584\u77e5\u8b58\u6307\u793a\uff0c\u7136\u59cb\u898b\u8def\u3002\u4f5b\u6559\u5982\u662f\uff0c\u666e", "418": "\u9858\u52ff\u7591\uff0c\u6545\u4ee5\u8a18\u8033\u3002", "419": "\u91d1\u525b\u7d93\u6ce8\u980c\u52d8\u6821\u91cb\u7562\u3002", "42": "\u8b9a\u66f0\uff1a", "420": "\u3014\u9304\u6587\u5b8c\u3015", "43": "\u8b49\u7a7a\u4fbf\u70ba\u5be6\uff0c\u57f7\u6211\u4e43\u540d\u865b\u3002", "44": "\u975e\u7a7a\u4ea6\u975e\u6211\uff0c\u8ab0\u6709\u5fa9\u8ab0\u7121\uff1f", "45": "\u5c0d\u75c5\u61c9\u65bd\u85e5\uff0c\u7121\u75c5\u85e5\u9084\u795b\u3002", "46": "\u5e38\u89c0\u4e8c\u7a7a\u7406\uff0c\u7a4e\u812b\u5165\u7121\u9918\u3002", "47": "\u82e5\u83e9\u85a9\u5fc3\u4f4f\u65bc\u6cd5\u800c\u884c\u5e03\u65bd\uff0c\u5982\u4eba\u5165\u6697\uff0c\u5247\u7121\u6240", "48": "\u898b\u3002\u6b64\u662f\u80cc\u660e\u5411\u6697\uff0c\u773e\u751f\u4e4b\u5fc3\u672c\u81ea\u7121\u4f4f\uff0c\u7121\u4f4f\u4e4b", "49": "\u5fc3\uff0c\u5373\u662f\u83e9\u63d0\u3002\u4e8c\u4e58\u4e4b\u4eba\uff0c\u80cc\u83e9\u63d0\u8def\uff0c\u884c\u51e1\u592b\u9053\uff0c", "5": "\u50b7\u5485\uff0c\u65bc\u6b64\u4e0d\u5f97\u751f\u6709\u4f4f\u4e4b\u5fc3\uff0c\u884c\u4eba\u4f4f\u6cd5\uff0c\u4ea6\u5fa9\u5982", "50": "\u5982\u4eba\u80cc\u660e\u5411\u6697\uff0c\u7336\u5982\u8ff7\u4eba\uff0c\u5982\u6b64\u4e4b\u4eba\uff0c\u84cb\u4e0d\u53ef\u8a00\u3002", "51": "\u82e5\u83e9\u85a9\uff0c\u5fc3\u4e0d\u4f4f\u6cd5\uff0c\u800c\u884c\u5e03\u65bd\uff0c\u5982\u4eba\u6709\u76ee\uff0c\u65e5", "52": "\u5149\u660e\u7167\uff0c\u898b\u7a2e\u7a2e\u8272\u3002\u91cb\u66f0\uff1a\u4e8c\u4e58\u4e0d\u898b\u8272\u4f4f\u8272\uff0c\u8b6c\u5982", "53": "\u4e0d\u898b\u5751\u843d\u5751\uff0c\u83e9\u85a9\u898b\u8272\u4e0d\u4f4f\u8272\uff0c\u5982\u4eba\u898b\u5751\u4e0d\u5165\u5751\u3002", "54": "\u4e00\u5207\u8af8\u6cd5\u4f46\u6709\u5047\u540d\uff0c\u4e8c\u4e58\u4e4b\u4eba\u70ba\u7121\u60e0\u76ee\uff0c\u4e0d\u8faf\u865b", "55": "\u5047\u3002\u83e9\u85a9\u6709\u76ee\uff0c\u80fd\u8faf\u771f\u5047\uff0c\u898b\u7a2e\u7a2e\u8272\uff0c\u6089\u7686\u7121\u76f8\u3002", "56": "\u9054\u6469\u5927\u5e2b\u4e91\uff1a\u300c\u4e0d\u898b\u8272\u662f\u898b\u8272\u300d\u3002", "57": "\u53d7\u6301\u8b80\u8aa6\u3002\u5e38\u898b\u81ea\u6027\u540d\u70ba\u53d7\u6301\uff0c\u53e3\u5e38\u8a18\u4e4b\u540d\u70ba", "58": "\u8b80\u8aa6\u3002", "59": "\u521d\u65e5\u5206\u4e43\u81f3\u4e2d\u65e5\u5206\uff0c\u4ee5\u6052\u6cb3\u6c99\u7b49\u8eab\u5e03\u65bd\u3002\u82e5\u7d04", "6": "\u662f\uff0c\u82e5\u4f4f\u4e00\u6cd5\uff0c\u5373\u88ab\u6cd5\u7e1b\uff0c\u4e0d\u514d\u751f\u6b7b\u3002\u662f\u6545\u83e9\u85a9\u65bc", "60": "\u4e8b\u76f8\u89e3\u8005\uff0c\u7957\u662f\u4e00\u65e5\u4e4b\u4e2d\u4e09\u6642\u6368\u8eab\u547d\uff0c\u6c42\u7121\u4e0a\u83e9", "61": "\u63d0\uff0c\u4ea6\u4e91\u4e09\u4e16\u6368\u8eab\u547d\uff0c\u6c42\u83e9\u63d0\u9053\uff0c\u4ea6\u4e91\u4e09\u5927\u963f\u50e7", "62": "\u8879\u52ab\u6368\u8eab\u547d\uff0c\u6c42\u83e9\u63d0\u9053\u3002\u6b64\u7686\u70ba\u8ff7\uff0c\u8aaa\u65bc\u56e0\u5730\uff0c\u975e", "63": "\u70ba\u609f\u8005\uff0c\u9813\u6301\u6b64\u7d93\u3002\u82e5\u7d04\u609f\u4eba\uff0c\u89c0\u4e09\u4e16\u7a7a\uff0c\u5373\u662f\u4e09", "64": "\u6642\u6368\u8eab\u547d\uff0c\u521d\u65e5\u5206\u662f\u904e\u53bb\u4e16\uff0c\u5f8c\u65e5\u5206\u662f\u672a\u4f86\u4e16\uff0c\u4e2d", "65": "\u65e5\u5206\u662f\u73fe\u5728\u4e16\uff0c\u96d6\u4f5c\u4e09\u4e16\u89c0\uff0c\u4ea6\u4e0d\u53ca\u5e38\u898b\u81ea\u6027\u7fa9\u3002", "66": "\u66f0\uff1a\u4e09\u4e16\u81ea\u7a7a\uff0c\u4f55\u5047\u65bc\u89c0\uff1f\u5e38\u898b\u81ea\u6027\u8005\uff0c\u5373\u5e38\u898b", "67": "\u4f5b\u3002\u7d93\u4e91\uff1a\u300c\u77bb\u4ef0\u5c0a\u984f\uff0c\u76ee\u4e0d\u66ab\u820d\u300d\u3002\u6b64\u5373\u662f\u4fe1\u5fc3", "68": "\u4e0d\u9006\u3002", "69": "\u66f8\u5beb\u3002\u4e86\u4e86\u898b\u6027\uff0c\u76ee\u4e4b\u70ba\u66f8\uff1b\u958b\u793a\u65bc\u4eba\uff0c\u6545\u540d", "7": "\u8af8\u6cd5\u4e2d\u61c9\u751f\u7121\u6240\u4f4f\u5fc3\u3002\u4e0b\u6587\u4e91\uff1a", "70": "\u70ba\u5beb\u3002\u773e\u751f\u5f97\u609f\uff0c\u8a18\u4e4b\u65bc\u5fc3\uff0c\u5ff5\u5ff5\u6642\u4e2d\u66f4\u7121\u5984\u5931\uff0c", "71": "\u6545\u540d\u66f8\u5beb\u3002", "72": "\u53d7\u6301\u3002\u842c\u6cd5\u5728\u6027\uff0c\u540d\u4e4b\u70ba\u53d7\uff1b\u6cd5\u6e05\u6de8\u6545\uff0c\u76ee\u4e4b", "73": "\u70ba\u6301\uff1b\u6545\u540d\u53d7\u6301\u3002", "74": "\u8b80\u8aa6\u3002\u5728\u5fc3\u66f0\u8b80\uff0c\u5728\u53e3\u66f0\u8aa6\uff0c\u5fc3\u53e3\u76f8\u61c9\uff0c\u6545\u540d", "75": "\u8b80\u8aa6\u3002", "76": "\u8b9a\u66f0\uff1a", "77": "\u773e\u751f\u8207\u58fd\u8005\uff0c\u860a\u4e0a\u5047\u865b\u540d\u3002", "78": "\u5982\u9f9c\u6bdb\u4e0d\u5be6\uff0c\u4f3c\u5154\u89d2\u7121\u5f62\u3002", "79": "\u6368\u8eab\u7336\u5984\u8b58\uff0c\u65bd\u547d\u70ba\u8ff7\u60c5\u3002", "8": "\u82e5\u5fc3\u6709\u4f4f\uff0c\u5247\u70ba\u975e\u4f4f\u3002\u91cb\u66f0\uff1a\u771f\u5982\u4e4b\u5fc3\uff0c\u672c\u81ea", "80": "\u8a73\u8ad6\u798f\u6bd4\u667a\uff0c\u4e0d\u53ca\u53d7\u6301\u7d93\u3002", "81": "\u5927\u4e58\u4e4b\u4eba\uff0c\u8272\u7a7a\u4ff1\u9063\uff0c\u4e0d\u8457\u4e8c\u908a\u3002\u4e8c\u908a\u65e2\u7121\uff0c", "82": "\u4e2d\u9053\u4f55\u7acb\uff1f\u4e00\u5207\u4e0d\u62d8\u3001\u4e0d\u67d3\uff0c\u842c\u5883\u5373\u662f\u5927\u4e58\u3002", "83": "\u6700\u4e0a\u4e58\u8005\u3002\u7121\u9063\u53ef\u9063\uff0c\u4ea6\u4e0d\u8a00\u7121\u9063\uff0c\u4ea6\u4e0d\u8a00\u62d8", "84": "\u8207\u4e0d\u62d8\uff0c\u8a00\u8a9e\u9053\u65b7\uff0c\u5fc3\u884c\u8655\u6ec5\uff0c\u6b64\u540d\u6700\u4e0a\u4e58\u3002\u5927", "85": "\u4e58\uff0c\u662f\u83e9\u85a9\u6240\u884c\u4e4b\u8def\u3002\u6700\u4e0a\u4e58\u8005\uff0c\u5373\u662f\u8af8\u4f5b\u4f4d\u5730\u3002", "86": "\u8b9a\u66f0\uff1a", "87": "\u6240\u4f5c\u7686\u4f9d\u6027\uff0c\u4fee\u6210\u529f\u5fb7\u6797\u3002", "88": "\u7d42\u7121\u8da3\u5bc2\u610f\uff0c\u552f\u6709\u6fdf\u7fa4\u5fc3\u3002", "89": "\u884c\u60b2\u60b2\u5ee3\u5927\uff0c\u7528\u667a\u667a\u6167\u6df1\u3002", "9": "\u7121\u4f4f\uff0c\u82e5\u4e0d\u4f4f\u8af8\u6cd5\uff0c\u5247\u8207\u9053\u76f8\u61c9\uff0c\u82e5\u4f4f\u4e00\u6cd5\uff0c\u5373\u9023", "90": "\u5229\u4ed6\u517c\u81ea\u5229\uff0c\u5c0f\u8056\u8a4e\u582a\u4efb\u3002", "91": "\u8377\u64d4\u5982\u4f86\u963f\u8028\u83e9\u63d0\u3002\u5373\u662f\u5927\u4e58\u4e4b\u4eba\uff0c\u64d4\u8005\u662f\u8377", "92": "\u8ca0\u4e4b\u7fa9\u4e5f\u3002\u4f9d\u6cd5\u4fee\u884c\u5373\u540d\u8377\u64d4\u3002", "93": "\u6a02\u5c0f\u6cd5\u8005\u3002\u5373\u662f\u4e8c\u4e58\u4e4b\u4eba\uff0c\u6545\u8aaa\u8457\u6211\u898b\u3001\u4eba", "94": "\u898b\u3002", "95": "\u662f\u4eba\u5148\u4e16\u7f6a\u696d\uff0c\u61c9\u58ae\u60e1\u9053\u3002\u6b64\u4eba\u524d\u4e16\u6709\u7f6a\uff0c\u5408", "96": "\u58ae\u60e1\u9053\uff0c\u4eca\u4e16\u6301\u7d93\u3002\u6642\u6709\u4eba\u8a9e\u66f0\uff1a\u6c5d\u524d\u65e2\u4f5c\u4e86\u5982\u8a31", "97": "\u91cd\u7f6a\uff0c\u4eca\u6642\u6301\u7d93\uff0c\u6709\u4f55\u6240\u76ca\uff1f\u6301\u7d93\u4e4b\u6b21\uff0c\u88ab\u4eba\u8f15\u8ce4", "98": "\u3002\u6301\u7d93\u4e4b\u4eba\u4e0d\u807d\u662f\u8a9e\u5df2\uff0c\u8caa\u7d93\u6545\uff0c\u4e0d\u807d\u9918\u8072\u3002\u6b64\u4eba", "99": "\u96d6\u6709\u91cd\u7f6a\uff0c\u5176\u7f6a\u6d88\u6ec5\u3002\u66f4\u4e00\u91cb\uff0c\u524d\u5fc3\u8d77\u5984\uff0c\u5408\u7d50\u7f6a"}, "originalTitle": "Buddhist Texts not contained in the Tripitaka, Electronic version, No. 073d \u91d1\u525b\u7d93\u6ce8\u980c\u91cb", "englishTitle": "Buddhist Texts not contained in the Tripitaka, Electronic version, No. 073d \u91d1\u525b\u7d93\u6ce8\u980c\u91cb", "author": "\u9054\u7167\u6574\u7406", "language": "chinese", "source": "CBETA"}