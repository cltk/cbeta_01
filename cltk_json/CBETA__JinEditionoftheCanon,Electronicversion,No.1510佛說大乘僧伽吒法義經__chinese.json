u773e\u751f\u554f\u85e5\u738b\u8ecd\u83e9\u85a9\u8a00", "236": "\u4ec1\u8005\u6b64\u8af8\u5bf6\u81fa\u5fae\u5999\u82b1\u5ea7\u662f\u4f55\u7b49\u76f8", "237": "\u85e5\u738b\u8ecd\u8a00\u6b64\u662f\u6c5d\u7b49\u6240\u5750\u4e4b\u5ea7\u82e5\u5750", "238": "\u6b64\u5ea7\u901f\u81f3\u4f5b\u6240\u77bb\u79ae\u5982\u4f86\u5247\u80fd\u75be\u5f97", "239": "\u7518\u9732\u5999\u6cd5\u5e74\u5c11\u773e\u751f\u767d\u8a00\u4ec1\u8005\u6211\u7b49", "24": "\u5fc3\u884c\u83e9\u63d0\u9053\u6148\u611b\u65bc\u4e16\u9592", "240": "\u4e0d\u77e5\u5982\u4f86\u6240\u4f4f\u4e4b\u8655\u4ea6\u4e0d\u77e5\u5f7c\u6240\u884c", "241": "\u4e4b\u8def\u7576\u8a63\u4f55\u65b9\u79ae\u89b2\u5982\u4f86\u85e5\u738b\u8ecd\u83e9", "242": "\u85a9\u8a00\u5584\u7537\u5b50\u5982\u4f86\u5c0a\u5fb7\u5982\u9808\udb8c\uddd0\u5c71\u5982", "243": "\u4f86\u667a\u6167\u6df1\u5982\u5927\u6d77\u4e0d\u5e0c\u4f9b\u990a\u4e0d\u6a02\u79ae", "244": "\u656c\u96d6\u6709\u60e1\u9b54\u4f86\u4fb5\u4e0d\u5171\u8acd\u8ad6\u82e5\u80fd\u6b78", "245": "\u4f9d\u4f5b\u8005\u5247\u5f97\u4e0d\u5165\u6b7b\u9580\u5f97\u6cd5\u7deb\u6301\u9580", "246": "\u5341\u65b9\u8af8\u5927\u83e9\u85a9\u6b32\u898b\u5982\u4f86\u4ee5\u81ea\u667a\u529b", "247": "\u7a2e\u7a2e\u89c0\u5bdf\u5c1a\u4e0d\u80fd\u898b\u6cc1\u6c5d\u7b49\u90aa\u5584\u7537", "248": "\u5b50\u5982\u4f86\u5982\u7a7a\u4e2d\u5875\u7121\u6709\u4f4f\u8655\u6c5d\u7b49\u82e5", "249": "\u80fd\u4ee5\u6e05\u6de8\u5fc3\u52ff\u6709\u5e0c\u9858\u4f46\u884c\u79ae\u62dc\u5373", "25": "\u6551\u5ea6\u8af8\u773e\u751f\u589e\u9577\u6210\u4f5b\u7a2e", "250": "\u898b\u5982\u4f86\u5c12\u6642\u5e74\u5c11\u773e\u751f\u53d7\u6559\u965e\u5ea7\u9069", "251": "\u5750\u83ef\u5ea7\u5373\u898b\u5982\u4f86\u8eab\u771f\u91d1\u8272\u5982\u5927\u91d1", "252": "\u5c71\u4e09\u5341\u4e8c\u76f8\u516b\u5341\u96a8\u597d\u4ee5\u81ea\u838a\u56b4\u5a01", "253": "\u5fb7\u7279\u5c0a\u773e\u5584\u5177\u8db3\u5728\u5927\u773e\u4e2d\u5750\u5927\u84ee", "254": "\u83ef\u5bf6\u5e2b\u5b50\u5ea7\u662f\u8af8\u5e74\u5c11\u773e\u751f\u6b61\u559c\u8e0a", "255": "\u8e8d\u5373\u8d77\u5408\u638c\u4e00\u5fc3\u9802\u79ae\u77bb\u4ef0\u5c0a\u984f\u76ee", "256": "\u4e0d\u66ab\u6368\u3002", "257": "\u5c12\u6642\u4e16\u5c0a\u51fa\u8fe6\u9675\u983b\u4f3d\u4e4b\u97f3\u8b9a\u8a00\u7ae5", "258": "\u5b50\u5584\u54c9\u5584\u54c9\u6c5d\u7b49\u80fd\u4ee5\u6e05\u6de8\u667a\u9858\u52c7", "259": "\u731b\u7cbe\u9032\u4f86\u6b64\u5a11\u5a46\u4e16\u754c\u79ae\u62dc\u89aa\u8fd1\u6211", "26": "\u7686\u4ee4\u5f97\u89e3\u812b\u901f\u6210\u7121\u4e0a\u6167", "260": "\u91cb\u8fe6\u725f\u5c3c\u4e16\u5c0a\u61c9\u6b63\u7b49\u89ba\u3002", "261": "\u5c12\u6642\u4e16\u5c0a\u7199\u6021\u5fae\u7b11\u5f9e\u5176\u9762\u9580\u51fa\u516b", "262": "\u4e07\u56db\u5343\u7a2e\u7a2e\u8272\u5149\u6240\u8b02\u9752\u9ec3\u8d64\u767d\u7d05", "263": "\u7d2b\u7e39\u8272\u9817\u80dd\u8fe6\u8272\u5fa7\u7167\u4e09\u5343\u5927\u5343\u4e16", "264": "\u754c\u4e0a\u81f3\u963f\u8fe6\u5c3c\u5412\u5929\u4e0b\u53ca\u8af8\u60e1\u5730\u7344", "265": "\u9047\u65af\u5149\u7167\u773e\u751f\u7686\u7372\u4e00\u5207\u5229\u76ca\u5b89\u6a02", "266": "\u5176\u5149\u7167\u5df2\u9084\u81f3\u4f5b\u6240\u9076\u4f5b\u4e03\u8fca\u5f9e\u4f5b", "267": "\u9802\u5165\u3002", "268": "\u5c12\u6642\u85e5\u738b\u8ecd\u83e9\u85a9\u5f9e\u5ea7\u800c\u8d77\u5408\u638c\u5411", "269": "\u4f5b\u767d\u8a00\u4e16\u5c0a\u5982\u4f86\u6240\u70ba\u751a\u6df1\u7a00\u6709\u5fc5", "27": "\u5c12\u6642\u85e5\u738b\u8ecd\u83e9\u85a9\u6469\u8a36\u85a9\u8207\u4e94\u767e\u5927", "270": "\u7121\u865b\u8a2d\u4eca\u65e5\u5fae\u7b11\u7b11\u5fc5\u6709\u56e0\u60df\u9858\u6148", "271": "\u60b2\u70ba\u6211\u958b\u8aaa\u4f5b\u544a\u85e5\u738b\u8ecd\u6c5d\u4eca\u80fd\u65bc", "272": "\u5927\u773e\u4e4b\u4e2d\u4f5c\u5927\u5e2b\u5b50\u543c\u554f\u65bc\u5982\u4f86\u5982", "273": "\u6b64\u7a00\u6709\u5fae\u5999\u5927\u4e8b\u6c5d\u4eca\u8ae6\u807d\u70ba\u6c5d\u89e3", "274": "\u8aaa\u5584\u7537\u5b50\u6c5d\u4eca\u898b\u6b64\u4e09\u5343\u4e03\u5bf6\u5927\u83ef", "275": "\u5ea7\u4e0d\u85e5\u738b\u8ecd\u8a00\u552f\u7136\u5df2\u898b\u4f5b\u8a00\u5584\u7537", "276": "\u5b50\u4eca\u6b64\u83ef\u5ea7\u662f\u8af8\u5e74\u5c11\u773e\u751f\u5f97\u5750\u6b64", "277": "\u5ea7\u5373\u7372\u5713\u6eff\u4e00\u5207\u5584\u6839\u8b49\u5f97\u4e00\u5207\u5584", "278": "\u6cd5\u6a02\u4f4f\u767b\u4e8e\u5341\u5730\u4e43\u81f3\u7576\u5f97\u963f\u8028\u591a", "279": "\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u5efa\u5927\u6cd5\u5e62\u64ca\u5927\u6cd5\u9f13", "28": "\u83e9\u85a9\u6469\u8a36\u85a9\u773e\u5404\u5404\u904b\u5927\u795e\u529b\u5f9e\u5ea7", "280": "\u7121\u91cf\u5929\u4eba\u805e\u662f\u6cd5\u5df2\u6089\u7372\u5229\u76ca\u7121\u91cf", "281": "\u5730\u7344\u773e\u751f\u4ed7\u662f\u6cd5\u529b\u89e3\u812b\u82e6\u5831\u5f97\u5b89", "282": "\u6a02\u8655\u662f\u6642\u6703\u4e2d\u6709\u4e5d\u5341\u9164\u80dd\u8af8\u8001\u773e", "283": "\u751f\u5f97\u9808\u9640\u6d39\u679c\u4f5b\u544a\u85e5\u738b\u8ecd\u6c5d\u4e14\u89c0", "284": "\u662f\u56db\u65b9\u4e0a\u4e0b\u8af8\u5927\u83e9\u85a9\u3002", "285": "\u5c12\u6642\u85e5\u738b\u8ecd\u83e9\u85a9\u5373\u89c0\u56db\u65b9\u898b\u6771\u65b9", "286": "\u754c\u4e94\u5341\u9164\u80dd\u6b91\u4f3d\u6c99\u83e9\u85a9\u800c\u4f86\u5411\u6b64", "287": "\u5357\u65b9\u754c\u516d\u5341\u9164\u80dd\u6b91\u4f3d\u6c99\u83e9\u85a9\u800c\u4f86", "288": "\u5411\u6b64\u897f\u65b9\u754c\u4e03\u5341\u9164\u80dd\u6b91\u4f3d\u6c99\u83e9\u85a9", "289": "\u800c\u4f86\u5411\u6b64\u5317\u65b9\u754c\u516b\u5341\u9164\u80dd\u6b91\u4f3d\u6c99", "29": "\u800c\u8d77\u6d8c\u8eab\u65bc\u865b\u7a7a\u4e2d\u73fe\u5927\u5a01\u5100\u5f9e\u5bb9", "290": "\u83e9\u85a9\u800c\u4f86\u5411\u6b64\u4e0b\u65b9\u754c\u4e5d\u5341\u9164\u80dd\u6b91", "291": "\u4f3d\u6c99\u83e9\u85a9\u800c\u4f86\u5411\u6b64\u4e0a\u65b9\u754c\u767e\u9164\u80dd", "292": "\u6b91\u4f3d\u6c99\u83e9\u85a9\u800c\u4f86\u5411\u6b64\u662f\u8af8\u5927\u83e9\u85a9", "293": "\u773e\u5230\u6b64\u6703\u4e2d\u606d\u656c\u4f9b\u990a\u6b61\u559c\u8b9a\u6b4e\u982d", "294": "\u9762\u79ae\u4e16\u5c0a\u8db3\u9076\u4f5b\u4e09\u8fca\u5404\u4f4f\u4e00\u9762\u85e5", "295": "\u738b\u8ecd\u83e9\u85a9\u767d\u4f5b\u8a00\u4e16\u5c0a\u6211\u4eca\u898b\u662f\u56db", "296": "\u65b9\u4e0a\u4e0b\u8af8\u5927\u83e9\u85a9\u4f86\u6b64\u6703\u4e2d\u4eca\u865b\u7a7a", "297": "\u4e2d\u5ffd\u7136\u898b\u6709\u9ed1\u8272\u9ec3\u8272\u7a2e\u7a2e\u76f8\u73fe\u5fa9", "298": "\u662f\u4f55\u7de3\u9858\u4f5b\u958b\u8aaa\u4f5b\u544a\u85e5\u738b\u8ecd\u6b64\u662f", "299": "\u60e1\u9b54\u53ca\u8af8\u7737\u5c6c\u6b32\u4f86\u81f3\u6b64\u70ba\u8af8\u5e74\u5c11", "3": "\u5f80\u6614\u5584\u53cb\u5316\u5c0e\u56e0\u7de3\u5df2\u5fa9\u544a\u85e5\u738b\u8ecd", "30": "\u8a73\u5e8f\u7de9\u6b65\u800c\u884c\u5176\u5fc3\u6c7a\u5b9a\u5b89\u56fa\u5927\u4e58", "300": "\u773e\u751f\u53d7\u662f\u4f4d\u6545\u6b32\u4f5c\u969c\u96e3\u7834\u58de\u662f\u4e8b", "301": "\u6545\u4f86\u81f3\u6b64\u85e5\u738b\u8ecd\u6c5d\u9069\u898b\u662f\u56db\u65b9\u4e0a", "302": "\u4e0b\u7121\u6578\u8af8\u5927\u83e9\u85a9\u7a2e\u7a2e\u8272\u76f8\u7a2e\u7a2e\u5f62", "303": "\u8c8c\u7a2e\u7a2e\u5a01\u529b\u8005\u6545\u4f86\u64c1\u8b77\u5e74\u5c11\u773e\u751f", "304": "\u4ee4\u5176\u60e1\u9b54\u53ca\u8af8\u7737\u5c6c\u7121\u80fd\u5f97\u4fbf\u662f\u60e1", "305": "\u9b54\u773e\u4ea6\u7686\u96a8\u559c\u615a\u6127\u800c\u9000\u4f5b\u8aaa\u662f\u7d93", "306": "\u5df2\u4e00\u5207\u52c7\u731b\u83e9\u85a9\u85e5\u738b\u8ecd\u83e9\u85a9\u8af8\u5927", "307": "\u83e9\u85a9\u7b49\u4e00\u5207\u8001\u773e\u751f\u5e74\u5c11\u773e\u751f\u8207\u7121", "308": "\u91cf\u5929\u4eba\u9f8d\u795e\u4e00\u5207\u5927\u773e\u7686\u5927\u6b61\u559c\u4fe1", "309": "\u53d7\u5949\u884c\u79ae\u4f5b\u800c\u9000\u3002", "31": "\u904a\u6232\u4e09\u6627\u73fe\u8af8\u795e\u8b8a\u6216\u73fe\u731b\u864e\u793a\u4ee5", "310": "\u4f5b\u8aaa\u5927\u4e58\u50e7\u4f3d\u5412\u6cd5\u7fa9\u7d93\u5377\u7b2c\u4e03", "32": "\u5a01\u96c4\u6216\u73fe\u5e2b\u5b50\u81ea\u5728\u7121\u754f\u6216\u73fe\u8c61\u738b", "33": "\u6709\u5927\u52e2\u529b\u4f4f\u6cd5\u5c71\u9802\u9ad8\u51fa\u7fa3\u5cef\u7121\u8207", "34": "\u7b49\u8005\u8eab\u653e\u5149\u660e\u6975\u5927\u71be\u76db\u767e\u5343\u9164\u80dd", "35": "\u65e5\u6708\u5149\u660e\u4e0d\u53ef\u8b6c\u55bb\u662f\u8af8\u83e9\u85a9\u653e\u6b64", "36": "\u5149\u660e\u884c\u65bc\u65e5\u6708\u4e4b\u524d\u5e74\u5c11\u773e\u751f\u898b\u662f", "37": "\u4e8b\u5df2\u767d\u5f7c\u4f5b\u8a00\u4e16\u5c0a\u4f55\u56e0\u7de3\u6545\u662f\u8af8", "38": "\u83e9\u85a9\u73fe\u662f\u7a00\u6709\u4e4b\u76f8\u653e\u5927\u5149\u660e\u884c\u65bc", "39": "\u65e5\u6708\u4e4b\u524d\u4f5b\u8a00\u5584\u7537\u5b50\u6c5d\u4eca\u898b\u662f\u4e8b", "4": "\u83e9\u85a9\u6469\u8a36\u85a9\u8a00\u85e5\u738b\u8ecd\u662f\u8af8\u82e6\u60f1\u773e", "40": "\u90aa\u5e74\u5c11\u773e\u751f\u767d\u4f5b\u8a00\u4e16\u5c0a\u552f\u7136\u5df2\u898b", "41": "\u4f5b\u8a00\u5584\u7537\u5b50\u662f\u8af8\u83e9\u85a9\u5404\u4ee5\u81ea\u5728\u795e", "42": "\u901a\u4e4b\u529b\u73fe\u5927\u5149\u660e\u5fa7\u7167\u4e16\u9592\u5e38\u8aaa\u5999", "43": "\u6cd5\u5316\u8af8\u773e\u751f\u6b32\u4ee4\u4e00\u5207\u773e\u751f\u7686\u5f97\u5229", "44": "\u76ca\u5b89\u6a02\u82e5\u8af8\u4eba\u5929\u4ea6\u80fd\u884c\u6b64\u5999\u884c\u4ea6", "45": "\u5f97\u5982\u662f\u795e\u901a\u8b8a\u5316\u53ca\u5927\u5149\u660e\u80fd\u884c\u65e5", "46": "\u6708\u4e4b\u524d\u5c12\u6642\u85e5\u738b\u8ecd\u83e9\u85a9\u7b49\u5f9e\u7a7a\u4e2d", "47": "\u4e0b\u524d\u8a63\u4f5b\u6240\u554f\u8a0a\u4e16\u5c0a\u4e16\u5c0a\u544a\u8a00\u85e5", "48": "\u738b\u8ecd\u6c5d\u7b49\u898b\u662f\u4e09\u5343\u5927\u5343\u4e16\u754c\u516d\u7a2e", "49": "\u9707\u52d5\u4e0d\u85e5\u738b\u8ecd\u83e9\u85a9\u767d\u4f5b\u8a00\u4e16\u5c0a\u552f", "5": "\u751f\u81e8\u547d\u7d42\u6642\u4ee5\u60e1\u696d\u6545\u53d7\u65af\u82e6\u5831\u96d6", "50": "\u7136\u5df2\u898b\u7136\u4eca\u6211\u7b49\u4e0d\u80fd\u4e86\u77e5\u4f55\u56e0\u4f55", "51": "\u7de3\u6709\u6b64\u9707\u52d5\u6cc1\u4eca\u6211\u7b49\u4ea6\u6709\u5c11\u7591\u6b32", "52": "\u554f\u4e16\u5c0a\u9858\u4f5b\u6148\u60b2\u807d\u6211\u6240\u554f\u958b\u793a\u6f14", "53": "\u8aaa\u5f7c\u4f5b\u544a\u8a00\u5584\u7537\u5b50\u6c5d\u6709\u6240\u7591\u6063\u6c5d", "54": "\u70ba\u554f\u6216\u904e\u53bb\u6216\u73fe\u5728\u6216\u672a\u4f86\u7b49\u4e8b\u6211", "55": "\u7576\u4e00\u4e00\u70ba\u6c5d\u5206\u5225\u89e3\u8aaa\u85e5\u738b\u8ecd\u83e9\u85a9", "56": "\u767d\u4f5b\u8a00\u4e16\u5c0a\u4e14\u4eca\u6b64\u6703\u4e2d\u6709\u516b\u4e07\u56db", "57": "\u5343\u5929\u5b50\u800c\u4f86\u96c6\u6703\u516b\u4e07\u56db\u5343\u9164\u80dd\u83e9", "58": "\u85a9\u773e\u4ea6\u4f86\u96c6\u6703\u4e00\u4e07\u4e8c\u5343\u9164\u80dd\u9f8d\u738b", "59": "\u4ea6\u4f86\u96c6\u6703\u5fa9\u6709\u4e00\u4e07\u516b\u5343\u795e\u738b\u4e8c\u5341", "6": "\u6709\u7236\u6bcd\u59bb\u5b50\u7737\u5c6c\u5167\u5916\u89aa\u8b58\u4e0d\u80fd\u6551", "60": "\u4e94\u5343\u9164\u80dd\u9913\u9b3c\u6bd7\u820d\u906e\u7b49\u4ff1\u4f86\u96c6\u6703", "61": "\u5176\u6578\u751a\u591a\u4f55\u56e0\u7de3\u6545\u800c\u5171\u4f86\u96c6\u6b32\u4f55", "62": "\u6240\u4f5c\u4f5b\u8a00\u5584\u7537\u5b50\u6c5d\u4eca\u8ae6\u807d\u7576\u77e5\u6b64", "63": "\u8af8\u5927\u773e\u4ff1\u4f86\u96c6\u6703\u7686\u662f\u65bc\u6b64\u6a02\u6b32\u807d", "64": "\u805e\u4f5b\u6240\u8aaa\u6cd5\u5373\u65bc\u4eca\u65e5\u6709\u5f97\u805e\u4f5b\u6df1", "65": "\u5999\u6cd5\u97f3\u6b63\u6167\u958b\u89ba\u65bc\u7169\u60f1\u7e1b\u5373\u5f97\u89e3", "66": "\u812b\u8005\u6216\u6709\u5f97\u805e\u5982\u4f86\u5999\u5531\u96e2\u751f\u8001\u75c5", "67": "\u6b7b\u7a2e\u7a2e\u8af8\u82e6\u4f4f\u5b89\u6a02\u6cd5\u8005\u6216\u6709\u5373\u4eca", "68": "\u7372\u5927\u5584\u5229\u51fa\u82e6\u8f2a\u8ff4\u5b89\u4f4f\u5341\u5730\u4f4f\u5341", "69": "\u5730\u5df2\u5f97\u8b49\u5982\u4f86\u6e05\u6de8\u6d85\u69c3\u65bc\u8af8\u82e6\u60f1", "7": "\u8b77\u82e5\u7121\u5584\u7a2e\u4f55\u7531\u89e3\u812b\u5c12\u6642\u4e16\u5c0a\u5fa9", "70": "\u6c38\u5f97\u89e3\u812b\u6d88\u4f0f\u9b54\u51a4\u8b49\u771f\u5999\u89ba\u8005\u85e5", "71": "\u738b\u8ecd\u83e9\u85a9\u5fa9\u767d\u4f5b\u8a00\u4e16\u5c0a\u5927\u5730\u516d\u7a2e", "72": "\u9707\u52d5\u5fa9\u662f\u4f55\u7b49\u745e\u76f8\u4f5b\u544a\u85e5\u738b\u8ecd\u6c5d", "73": "\u4eca\u5be9\u8ae6\u89c0\u5bdf\u4e91\u4f55\u9707\u52d5\u85e5\u738b\u8ecd\u83e9\u85a9", "74": "\u5fa9\u89c0\u5bdf\u5df2\u767d\u4f5b\u8a00\u4e16\u5c0a\u6211\u5df2\u89c0\u5bdf\u898b", "75": "\u662f\u56db\u65b9\u53ca\u8af8\u65b9\u6240\u6709\u4e8c\u5341\u9164\u80dd\u773e\u751f", "76": "\u5ffd\u7136\u751f\u65bc\u6b64\u9592\u5fa9\u6709\u4e0b\u65b9\u4e8c\u5341\u9164\u80dd", "77": "\u773e\u751f\u5f9e\u5730\u6d8c\u51fa\u751f\u65bc\u6b64\u9592\u4e0a\u65b9\u6709\u4e8c", "78": "\u5341\u4e94\u9164\u80dd\u773e\u751f\u4f86\u751f\u6b64\u9592\u7531\u662f\u5927\u5730", "79": "\u516d\u7a2e\u9707\u52d5\u4e0d\u77e5\u6b64\u7b49\u662f\u4f55\u745e\u76f8\u4f5b\u544a", "8": "\u8aaa\u5048\u8a00\u3002", "80": "\u85e5\u738b\u8ecd\u4e14\u5f85\u9808\udb84\udd68\u6c5d\u81ea\u4e86\u77e5\u4e8e\u6642\u5e74", "81": "\u5c11\u773e\u751f\u898b\u662f\u516d\u5341\u4e94\u9164\u80dd\u773e\u751f\u5ffd\u7136", "82": "\u4f86\u751f\u6b64\u9592\u767d\u4f5b\u8a00\u4e16\u5c0a\u4e91\u4f55\u6b64\u4e2d\u5ffd", "83": "\u751f\u5982\u662f\u773e\u751f\u4f5b\u5fa9\u544a\u8a00\u5584\u7537\u5b50\u6b64\u7b49", "84": "\u773e\u751f\u7686\u662f\u6c5d\u4e4b\u5f92\u4fb6\u4f86\u751f\u6b64\u9592\u5e74\u5c11", "85": "\u773e\u751f\u767d\u4f5b\u8a00\u4e16\u5c0a\u6b64\u7b49\u773e\u751f\u4f86\u751f\u6b64", "86": "\u9592\u4ea6\u6709\u6b7b\u4e0d\u4f5b\u544a\u5e74\u5c11\u773e\u751f\u4e00\u5207\u773e", "87": "\u751f\u6089\u7686\u6709\u6b7b\u6b64\u4ea6\u6709\u6b7b\u6642\u8af8\u5e74\u5c11\u773e", "88": "\u751f\u5408\u638c\u5411\u4f5b\u9802\u79ae\u4f5b\u8db3\u767d\u8a00\u4e16\u5c0a\u6211", "89": "\u7b49\u4eca\u8005\u66f4\u4e0d\u80fd\u5fcd\u53d7\u5982\u662f\u751f\u6b7b\u6d41\u8f49", "9": "\u82e5\u4f5c\u8af8\u60e1\u696d\u58ae\u5927\u5730\u7344\u4e2d", "90": "\u9858\u901f\u89e3\u812b\u4f5b\u544a\u5e74\u5c11\u773e\u751f\u6c5d\u7b49\u80fd\u8d77", "91": "\u5927\u7cbe\u9032\u4e0d\u5e74\u5c11\u773e\u751f\u767d\u4f5b\u8a00\u4e16\u5c0a\u6211", "92": "\u7b49\u4eca\u8005\u89aa\u89a9\u5982\u4f86\u91d1\u8272\u4e4b\u8eab\u5f97\u805e\u5982", "93": "\u4f86\u7518\u9732\u5999\u6cd5\u898b\u8af8\u83e9\u85a9\u6469\u8a36\u85a9\u7b49\u73fe", "94": "\u5927\u795e\u901a\u8af8\u8072\u805e\u773e\u7121\u91cf\u5929\u4eba\u96c6\u6703\u807d", "95": "\u6cd5\u9858\u81ea\u7cbe\u9032\u61c3\u4fee\u8056\u9053\u4e0d\u80fd\u5fcd\u53d7\u751f", "96": "\u6b7b\u6d41\u8f49\u6642\u8af8\u5e74\u5c11\u773e\u751f\u53c8\u5fa9\u9076\u4f5b\u4e09", "97": "\u8fca\u504f\u8892\u53f3\u80a9\u6147\u61c3\u606d\u656c\u79ae\u4e16\u5c0a\u8db3\u91cd", "98": "\u767d\u4f5b\u8a00\u4e16\u5c0a\u6211\u7b49\u773e\u751f\u773e\u82e6\u6240\u96c6\u751a", "99": "\u5927\u71b1\u60f1\u65bc\u751f\u6b7b\u91cd\u6a90\u6975\u5927\u6016\u754f\u4f55\u7531"}, "originalTitle": "Jin Edition of the Canon, Electronic version, No. 1510 \u4f5b\u8aaa\u5927\u4e58\u50e7\u4f3d\u5412\u6cd5\u7fa9\u7d93", "englishTitle": "Jin Edition of the Canon, Electronic version, No. 1510 \u4f5b\u8aaa\u5927\u4e58\u50e7\u4f3d\u5412\u6cd5\u7fa9\u7d93", "author": "\u5b8b \u91d1\u7e3d\u6301\u7b49\u8b6f", "language": "chinese", "source": "CBETA"}