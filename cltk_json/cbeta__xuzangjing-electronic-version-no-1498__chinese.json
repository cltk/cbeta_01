u6b63\u9762\u3002\u591a\u756b\u5168\u8eab\u3002\u5e38\u4f9d\u6cd5\u6703\u3002\u8eab\u5fc3\u5584\u826f\u3002\u6f38\u9732", "182": "\u5713\u6eff\u5149\u660e\u4e4b\u8c61\u3002", "183": "\u8b9a\u66f0\u3002", "184": "\u7dba\u4ea4\u8108\u6ce8\u975e\u751f\u975e\u4f5b\u4e00\u7247\u9748\u6a5f\u4f73\u54c9\udb8e\udf02\udb8e\udf02", "185": "\u4e2d\u6709\u9ee0\u8005\u9ede\u775b\u98db\u7a7a\u52a9\u6211\u7d93\u7db8\u6551\u6b64\u7fa3\u8499", "186": "\u724c\u5f0f\u724c\u5f0f", "187": "\u7b2c\u4e00\u5e2d\u5341\u724c\u25cb\u5341\u8eab\u9748\u76f8\u6cd5\u5831\u5316\u4e00\u5207\u4f5b\u5341\u724c\u7686\u7136", "188": "\u7b2c\u4e8c\u5e2d\u5341\u724c\u25cb\u5341\ud848\uddaf\u5fae\u5999\u904e\u73fe\u672a\u4e00\u5207\u6cd5\u540c\u4e0a", "189": "\u7b2c\u4e09\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u96f2\u96c6\u7a2e\u7a2e\u4e58\u4e00\u5207\u50e7\u540c\u4e0a", "190": "\u7b2c\u56db\u5e2d\u5341\u724c\u25cb\u5341\u5b97\u8af8\u7956\u5e2b\u50e7\u5927\u5584\u77e5\u8b58\u540c\u4e0a", "191": "\u7b2c\u4e94\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u5f80\u751f\u6de8\u57df\u8af8\u4e0a\u5584\u4eba\u540c\u4e0a", "192": "\u7b2c\u516d\u5e2d\u5341\u724c\u25cb\u5341\u524e\u5875\u84ee\u82b1\u85cf\u5999\u56b4\u4e16\u4e3b\u540c\u4e0a", "193": "\u7b2c\u4e03\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u5165\u4f5b\u5883\u754c\u5bc4\u4f4d\u4ed9\u4eba\u540c\u4e0a", "194": "\u7b2c\u516b\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u5165\u4f5b\u5883\u754c\u4e00\u5207\u5112\u5b97\u540c\u4e0a", "195": "\u7b2c\u4e5d\u5e2d\u5341\u724c\u25cb\u5341\u5927\u660e\u738b\u5341\u65b9\u793a\u73fe\u5973\u5929\u540c\u4e0a", "196": "\u7b2c\u5341\u5e2d\u5341\u724c\u25cb\u5341\u524e\u98db\u8eab\u91cd\u91cd\u8b77\u6cd5\u8af8\u795e\u540c\u4e0a", "197": "\u7b2c\u5341\u4e00\u5e2d\u5341\u724c\u25cb\u6c34\u9678\u672c\u5b97\u6b77\u4ee3\u8af8\u5584\u77e5\u8b58\u540c\u4e0a", "198": "\u4ee5\u4e0a\u3002\u4e0a\u5802\u7adf\u3002", "199": "\u4ee5\u4e0b\u3002\u4e0b\u5802\u5217\u4f4d\u3002", "200": "\u7b2c\u5341\u4e8c\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u4e00\u5207\u5929\u4e0a\u771f\u9748\u5341\u724c\u7686\u7136", "201": "\u7b2c\u5341\u4e09\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u8af8\u4ed9\u5217\u6d3e\u771f\u9748\u540c\u4e0a", "202": "\u7b2c\u5341\u56db\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u52dd\u52a3\u516b\u90e8\u771f\u9748\u540c\u4e0a", "203": "\u7b2c\u5341\u4e94\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u56db\u7a2e\u8f2a\u738b\u771f\u9748\u540c\u4e0a", "204": "\u7b2c\u5341\u516d\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u4e00\u5207\u4eba\u985e\u771f\u9748\u540c\u4e0a", "205": "\u7b2c\u5341\u4e03\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u963f\u4fee\u7f85\u90e8\u771f\u9748\u540c\u4e0a", "206": "\u7b2c\u5341\u516b\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u5225\u5225\u6210\u5c31\u7cbe\u9748\u540c\u4e0a", "207": "\u7b2c\u5341\u4e5d\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u5f97\u812b\u9b3c\u8da3\u4e4b\u9748\u540c\u4e0a", "208": "\u7b2c\u4e8c\u5341\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u5f97\u812b\u65c1\u5728\u4e4b\u9748\u540c\u4e0a", "209": "\u7b2c\u4e8c\u5341\u4e00\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u5f97\u812b\u6ce5\u7281\u4e4b\u9748\u540c\u4e0a", "210": "\u7b2c\u4e8c\u5341\u4e8c\u5e2d\u5341\u724c\u25cb\u5341\u65b9\u7121\u91cf\u6b7b\u751f\u4e2d\u6709\u4e4b\u9748\u540c\u4e0a", "211": "\u7b2c\u4e8c\u5341\u4e09\u5e2d\u5341\u724c\u25cb\u6b64\u4e00\u9053\u5834\u5341\u65b9\u6709\u8077\u8af8\u795e\u540c\u4e0a", "212": "\u7b2c\u4e8c\u5341\u56db\u5e2d\u5341\u724c\u25cb\u6b64\u4e00\u9053\u5834\u6b63\u5ea6\u5217\u4f4d\u771f\u9748\u540c\u4e0a", "213": "\u7b2c\u4e8c\u5341\u4e94\u5e2d\u5341\u724c\u25cb\u6b64\u4e00\u9053\u5834\u90e8\u4f4d\u4e00\u5207\u771f\u9748\u540c\u4e0a", "214": "\u552f\u4e8c\u5341\u56db\u5e2d\u4e8c\u5341\u4e94\u5e2d\u3002\u6bcf\u724c\u5c45\u4e2d\u5927\u5b57\u5916\u3002\u53ef\u8a73\u958b", "215": "\u6b63\u966a\u4eba\u540d\u800c\u5c0f\u5b57\u66f8\u4e4b\u3002\u7e3d\u5225\u540c\u7570\u6210\u58de\u3002\u80fd\u5168\u73fe\u65bc", "216": "\u4e00\u6bdb\u7aef\u4e5f\u3002\u5bf6\u738b\u524e\u958b\u3002\u6cd5\u8f2a\u5927\u8f49\u3002\u4ea6\u751f\u5e73\u4e4b\u5feb\u4e8b\u77e3\u3002", "217": "(\u9644)\u6c34\u9678\u9053\u5834\u9644\u7533\u6587\u758f\u5f0f\u9644\u6c34\u9678\u9053\u5834\u9644\u7533\u6587\u758f\u5f0f", "218": "\u6c34\u9678\u5167\u58c7\u6cd5\u4e8b\u6c99\u9580\u8b39\u758f\u3002\u4e09\u5bf6\u9f8d\u5929\u4e00\u5207\u771f\u9748\u5ea7\u4e0b\u3002", "219": "\u4f0f\u4ee5\u9053\u5834\u7121\u7919\u3002\u773e\u9858\u74b0\u8208\u3002\u65e2\u96c6\u6240\u9577\u5fc5\u9664\u5176\u60a3\u3002\u7a31\u6027", "220": "\u800c\u5468\u6cd5\u754c\u3002\u5f9e\u5fc3\u800c\u4f5c\u529f\u52db\u3002\u65e2\u6709\u611f\u800c\u80fd\u901a\u3002\u6545\u64da\u60c5\u4ee5", "221": "\u4e0a\u9054\u3002\u4eca\u6709\u9053\u5834\u96a8\u559c\u5584\u4fe1\u3002\u4e0b\u5217\u4eba\u540d\u53ca\u6240\u6c42\u5fc3\u9858\u4ed7\u4e09\u5bf6\u529b\u3002\u5180", "222": "\u5f97\u5982\u9858\u3002\u4e91\u4e91\u3002\u67d0\u7b49\u65e2\u53d7\u4f86\u8a5e\u3002\u7406\u5408\u577f\u758f\u3002\u4e0a\u5875\u6167\u9452\u3002\u771f", "223": "\u6148\u6717\u8000\u3002\u5fa7\u71ed\u660f\u8862\u3002\u6cd5\u4f4d\u5e38\u5b89\u3002\u6c38\u5b5a\u773e\u5fd7\u3002\u5408\u5341\u65b9\u5927\u60b2", "224": "\u795e\u8056\u4e4b\u529b\u3002\u6551\u773e\u751f\u5fc3\u4e2d\u82e6\u60f1\u4e4b\u7de3\u3002\u6240\u7f3a\u7686\u5713\u3002\u6240\u9ecf\u7686", "225": "\u812b\u3002\u751f\u8005\u8499\u798f\u3002\u6545\u8005\u8d85\u767b\u3002\u842c\u82e6\u9577\u96e2\u3002\u4e00\u771f\u5713\u73fe\u3002\u6240\u6709\u4e0b", "226": "\u60c5\u3002\u4f0f\u60df\u5468\u5bc6\u8b77\u6301\u3002\u4ee5\u6eff\u9053\u5834\u5168\u91cf\u3002\u8b39\u758f\u3002", "227": "\u5e74\u6708\u65e5\u5177\u758f\u6c34\u9678\u9053\u5834\u6cd5\u4e8b\u6c99\u9580\u3000\u62bc", "228": "\u6a80\u3000\u3000\u8d8a\u3000\u62bc", "229": "\u7c64\u4e0a\u5beb\u6c34\u9678\u9053\u5834\u4f5b\u6cd5\u50e7\u5bf6", "230": "\u6c34\u9678\u9053\u5834\u5404\u5bb6\u9644\u884c\ud85a\udda8\u53e3\u610f\u65e8\u5f0f\u6c34\u9678\u9053\u5834\u5404\u5bb6\u9644\u884c\ud85a\udda8\u53e3\u610f\u65e8\u5f0f", "231": "\u6c34\u9678\u9053\u5834\u3002\u79c9\u6559\u5949\u884c\u6cd5\u4e8b\u6c99\u9580\u3002\u4f0f\u4ee5\u5f9e\u5fc3\u73fe\u696d\u3002\u5373\u4e00", "232": "\u5373\u4e09\u3002\u7834\u5984\u986f\u771f\u3002\u8ab0\u5347\u8ab0\u964d\u3002\u70ba\u773e\u8ff7\u4e4b\u4e0d\u609f\u3002\u6545\u8af8\u4f5b\u4e43", "233": "\u964d\u81e8\u3002\u6167\u555f\u7121\u906e\u3002\u5149\u7167\u6709\u8d6b\u3002\u63a5\u5f15\u4e4b\u7de3\u5c07\u719f\u3002\u8b8a\u5316\u4e4b\u7528", "234": "\u5f4c\u591a\u3002\u7be4\u5949\u771f\u8a00\u3002\u606a\u9075\u5100\u8ecc\u3002\u666e\u65bd\u659b\u98df\u3002\u5168\u6fdf\u542b\u9748\u3002\u4eca\u6709", "235": "\u4e0b\u5217\u4eba\u540d\u5fc3\u9858\u4ed7\u4f5b\u6069\u9748\u3002\u4f7f\u5f7c\u8d85\u5ea6\u3002\u6089\u7686\u5982\u9858\u3002\u4e91\u4e91\u3002\u67d0\u7b49\u65e2", "236": "\u53d7\u4f86\u8a5e\u3002\u7406\u5408\u5982\u6cd5\u3002\u64da\u60c5\u4e0a\u9054\u3002\u4f0f\u9858\u751f\u5b58\u8499\u798f\u3002\u4ea1\u6545\u8d85", "237": "\u5347\u3002\u773e\u82e6\u5168\u96e2\u3002\u4e00\u771f\u5713\u73fe\u3002\u5f4c\u9640\u5fc3\u93e1\u4e0d\u9694\u7d72\u6beb\u3002\u6975\u6a02\u84ee", "238": "\u82b1\u5168\u901a\u6d88\u606f\u3002", "239": "\u5e74\u6708\u65e5\u5177", "240": "\u7c64\u4e0a\u5beb\u6c34\u9678\u9053\u5834\u71c4\u53e3\u610f\u65e8", "241": "\u6cd5\u754c\u8056\u51e1\u6c34\u9678\u5927\u9f4b\u666e\u5229\u9053\u5834\u6027\u76f8\u901a\u8ad6\u7b2c\u5377\u4e5d\u7d42"}, "englishTitle": "\u534d Xuzangjing, Electronic version, No. 1498 \u6cd5\u754c\u8056\u51e1\u6c34\u9678\u5927\u9f4b\u666e\u5229\u9053\u5834\u6027\u76f8\u901a\u8ad6", "author": "\u6e05 \u54ab\u89c0\u8ff0"}