884c\u5be6", "235": "\u5e2b\u8af1\u5927\u6085\u865f\u5929\u4e00\u695a\u5357\u5442\u6c0f\u5b50\u751f\u503c\u96e2\u4e82\u88ab\u64c4\u5165\u9ed4", "236": "\u5728\u5b89\u9806\u9577\u58fd\u83f4\u9047\u96f2\u8179\u548c\u5c1a\u4e0a\u5802\u56e0\u767c\u51fa\u5875\u4e4b\u5ff5\u6642", "237": "\u503c\u3002", "238": "\u672c\u671d\u5927\u5e2b\u6062\u53d6\u6ec7\u9ed4\u672c\u71df\u8ecd\u4f0d\u4e00\u6642\u661f\u6563\u5e2b\u559c\u812b\u7a4e", "239": "\u5954\u81f3\u5b89\u5357\u9f8d\u6f6d\u5bfa\u79ae\u4e0a\u7121\u4e0b\u971e\u8001\u5bbf\u62ab\u5243\u7d93\u534a\u8f09\u5f97", "240": "\u805e\u5584\u6b0a\u548c\u5c1a\u958b\u6cd5\u9f8d\u5834\u842c\u58fd\u5bfa\u5e2b\u5f80\u89aa\u89b2\u4e5e\u53d7\u5177\u8db3", "241": "\u6b0a\u4e00\u898b\u4ee5\u5668\u671f\u4e4b\u4e00\u65e5\u554f\u5e2b\u66f0\u6c5d\u4f86\u60df\u6c42\u6212\u8036\u66f4\u6c42", "242": "\u5225\u4e8b\u8036\u5e2b\u66f0\u67d0\u7532\u4e0d\u77e5\u6709\u4f55\u4e8b\u66f4\u6c42\u4e5e\u548c\u5c1a\u6148\u60b2\u958b", "243": "\u793a\u6b0a\u66f0\u6c5d\u8c48\u4e0d\u805e\u751f\u6b7b\u4e8b\u5927\u8036\u5e2b\u66f0\u751f\u6b7b\u4e8b\u5927\u4f5c\u9ebc", "244": "\u751f\u4e86\u6b0a\u66f0\u53c3\u7a76\u59cb\u5f97\u5e2b\u66f0\u4f5c\u9ebc\u751f\u53c3\u6b0a\u66f0\u7236\u6bcd\u672a\u751f", "245": "\u524d\u90a3\u7b87\u662f\u4f60\u672c\u4f86\u9762\u76ee\u6041\u9ebc\u53c3\u53bb\u5e2b\u79c9\u6b0a\u547d\u5982\u662f\u53c3", "246": "\u7a76\u523b\u7121\u9593\u65b7\u6642\u6b0a\u9077\u677e\u5dcb\u5e2b\u4ea6\u96a8\u4e4b\u4f4f\u7d93\u5169\u8f09\u4e00\u65e5", "247": "\u4f8d\u6b0a\u8207\u5b98\u4eba\u6301\u8ad6\u695e\u56b4\u7d93\u81f3\u4e16\u5c0a\u8209\u91d1\u8272\u81c2\u5c48\u4e94\u8f2a", "248": "\u6307\u554f\u963f\u96e3\u8655\u8c41\u7136\u6709\u9192\u81f3\u591c\u4e0a\u65b9\u4e08\u5448\u89e3\u66f0\u8001\u548c\u5c1a", "249": "\u7121\u7aef\u76f8\u779e\u67d0\u7532\u8a31\u591a\u6642\u4e5f\u6b0a\u66f0\u6c5d\u898b\u7b87\u751a\u9ebc\u9053\u7406\u4fbf", "250": "\u6041\u9ebc\u9053\u5e2b\u66f0\u4e16\u5c0a\u8209\u624b\u5c48\u6307\u793a\u963f\u96e3\u8655\u807b\u6b0a\u66f0\u6211\u60c5", "251": "\u77e5\u6c5d\u53ea\u8a8d\u5f97\u7b87\u9a62\u524d\u99ac\u5f8c\u5e95\u70ba\u6975\u5247\u5728\u5e2b\u66f0\u67d0\u7532\u53ea", "252": "\u6041\u9ebc\u66f4\u4e0d\u53d7\u8001\u548c\u5c1a\u779e\u4e5f\u6b0a\u53f1\u4e4b\u66f0\u5b5f\u516b\u90ce\u6f22\u4e0d\u898b", "253": "\u53e4\u4eba\u9053\u4f5b\u6cd5\u4e0d\u662f\u8005\u7b87\u9053\u7406\u5e2b\u66f0\u8001\u5927\u5584\u77e5\u8b58\u66f4\u8207", "254": "\u4eba\u8aaa\u9053\u7406\u5728\u6b0a\u6df1\u80af\u4e4b\u5e2b\u81ea\u53d7\u56d1\u5f8c\u7e3d\u672a\u96e2\u6b0a\u96a8\u4f4f", "255": "\u677e\u5dcb\u4e8c\u5341\u9918\u8f09\u7532\u5b50\u6b72\u6b0a\u793a\u5bc2\u672c\u5883\u6a80\u8d8a\u4e26\u5408\u5c71\u5927", "256": "\u773e\u52c9\u5e2b\u7e7c\u5e2d\u5e2b\u4e43\u7aed\u529b\u9f0e\u5efa\u677e\u5dcb\u524d\u5f8c\u5eca\u5ee1\u7165\u7136\u4e00", "257": "\u65b0\u53c8\u82e6\u5b88\u5341\u9918\u5e74\u6642\u9047\u4e01\u4e11\u5b5f\u51ac\u5076\u793a\u5fae\u6059\u4e00\u65e5\u793a", "258": "\u5f92\u5316\u6708\u66f0\u543e\u6ec5\u5f8c\u4e0d\u52de\u76e4\u85aa\u8cbb\u4e8b\u751f\u846c\u53ef\u4e5f\u8a00\u8a16\u800c", "259": "\u901d\u5316\u4e43\u4e0d\u9055\u5e2b\u547d\u5099\u68fa\u6536\u6bae\u5168\u8eab\u5efa\u5854\u65bc\u5bfa\u897f\u8001\u5854", "260": "\u5c71\u5b58\u70ba\u5e2b\u4e16\u58fd\u516d\u5341\u6709\u4e03\u50e7\u81d8\u56db\u5341\u3002", "261": "\u5854\u9298\u5854\u9298", "262": "\u4eca\u592b\u4e16\u7cfb\u76f8\u627f\u4fee\u7406\u585a\u5893\u793a\u5b50\u5b6b\u65bc\u4e0d\u5fd8\u6b64\u5728\u5875\u8005", "263": "\u5b5d\u5f1f\u4e4b\u5fc3\u4e5f\u81f3\u65bc\u51fa\u5875\u5ea6\u4e16\u5efa\u5857\u6bd2\u9f13\u6f14\u7a7a\u738b\u6cd5\u5b9c", "264": "\u4e4e\u842c\u6709\u7686\u7a7a\u7121\u4e8b\u585a\u5893\u7136\u800c\u5b5d\u5f1f\u4e4b\u601d\u7587\u4e0d\u6709\u4e4b\u96d6", "265": "\u5728\u91cb\u6c0f\u4ea6\u4e0d\u80fd\u5ee2\u6b64\u5929\u4e00\u79aa\u5e2b\u58fd\u5854\u4e4b\u6240\u4ee5\u5efa\u4e5f\u79aa", "266": "\u5e2b\u672c\u695a\u5357\u5442\u6c0f\u5b50\u56e0\u503c\u5175\u71f9\u88ab\u64c4\u5165\u71df\u6d89\u6b77\u8271\u8f9b\u7121", "267": "\u4e0d\u5099\u5617\u7b2c\u8eab\u96d6\u5728\u71df\u5fc3\u5247\u51fa\u5bb6\u5f8c\u4e8c\u5343\u9918\u6b72\u5352\u6210\u5176", "268": "\u5fd7\u50b3\u5584\u6b0a\u79aa\u5e2b\u8863\u7f3d\u5be6\u53c3\u9ed8\u609f\u51e1\u6709\u6240\u5f97\u4ff1\u5f9e\u6027\u5730", "269": "\u5713\u660e\u548c\u76e4\u6258\u51fa\u6240\u4ee5\u6216\u62c8\u63ed\u81ea\u6301\u6216\u8aaa\u6cd5\u793a\u4eba\u7d55\u53bb", "270": "\u652f\u96e2\u5be6\u9053\u672c\u4f86\u9762\u76ee\u95b1\u9053\u81d8\u56db\u5341\u9918\u5e74\u958b\u5802\u6f14\u5316\u50b3\u5176", "271": "\u6cd5\u8005\u82e5\u5e72\u4eba\u5229\u76ca\u4e8e\u4eba\u4e5f\u5927\u77e3\u8332\u58fd\u5854\u4e4b\u5efa\u5728\u5e2b\u4e0d", "272": "\u4ee5\u6709\u7121\u70ba\u8f15\u91cd\u800c\u5584\u4e00\u6cd5\u5f1f\u8cac\u5e2b\u6cd5\u55e3\u4f75\u53d7\u5e2b\u4e4b\u5229", "273": "\u76ca\u8005\u4e0d\u5fcd\u4e0d\u5efa\u5e2b\u4e4b\u58fd\u5854\u4ee5\u793a\u5f8c\u5b78\u65bc\u4e0d\u5fd8\u4e14\u76e1\u5176", "274": "\u5b5d\u5f1f\u4e4b\u5fc3\u4ee5\u8acb\u9298\u4e8e\u4e88\u4e5f\u56e0\u9298\u4e4b\u66f0\u3002", "275": "\u56db\u5927\u672c\u7a7a\u4e94\u860a\u975e\u6709\u83e9\u63d0\u7121\u6a39\u61f8\u5d16\u6492\u624b\u9707\u64bc\u6cb3\u5c71", "276": "\u5730\u4e2d\u7345\u543c\u4e00\u5e2b\u5854\u9802\u5713\u5149\u4e0d\u673d\u5484", "277": "\u7e61\u8c37\u5468\u540d\u9060\u8072\u53ca\u6c0f\u62dc\u64b0\u3002", "278": "\u4f5b\u4e8b\u4f5b\u4e8b", "279": "\u70ba\u592b\u5dd6\u6cd5\u5f1f\u8d77\u9f95\u4e91\u5929\u4e0a\u4eba\u9593\u7368\u8d85\u5353\u61f8\u5dd6\u6492\u624b\u81ea", "280": "\u627f\u7576\u62cb\u537b\u5ee3\u798f\u5c24\u85cf\u8de1\u7a81\u51fa\u5149\u660e\u6620\u516b\u8352\u4e14\u5bc6\u79fb\u4e00", "281": "\u6b65\u4f5c\u9ebc\u751f\u5546\u91cf\u4ee5\u62c2\u5b50\u6307\u5f91\u4e91\u5802\u5802\u3002", "282": "\u8209\u706b\u4e91\u4f5b\u7956\u7db1\u7dad\u8872\u50e7\u5df4\u9f3b\u4e16\u51fa\u4e16\u9593\u7531\u6b64\u5efa\u7acb\u4e14", "283": "\u9053\u5efa\u7acb\u7b87\u751a\u9ebc\u5efa\u6cd5\u5e62\u7acb\u898f\u77e9\u63a5\u65b9\u4f86\u935b\u8872\u5b50\u8005\u500b", "284": "\u4fbf\u662f\u6211\u5929\u5dd6\u6cd5\u5f1f\u4e8c\u4e09\u4e00\u5e74\u5728\u5ee3\u798f\u5bfa\u88cf\u6a6b\u958b\u8c4e\u95e2", "285": "\u4efb\u610f\u65bd\u70ba\u5e95\u4e00\u6bb5\u98a8\u5149\u4eca\u65e5\u79fb\u4f86\u8005\u500b\u5927\u706b\u805a\u88cf\u8981", "286": "\u8207\u8af8\u4eba\u901a\u7b87\u6d88\u606f\u6240\u4ee5\u6085\u4e0a\u5ea7\u4e0d\u5f97\u4e0d\u70ba\u4f0a\u767c\u660e\u4e14", "287": "\u5982\u4f55\u767c\u660e\u4ee5\u706b\u70ac\u6253\u5713\u76f8\u4e91\u70c8\u7130\u5149\u4e2d\u8f49\u6cd5\u8f2a\u3002", "288": "\u70ba\u518d\u64cd\u4e0a\u4eba\u8d77\u9f95\u4e91\u8d64\u689d\u689d\u5149\u707c\u707c\u4f86\u53bb\u81ea\u5206\u660e\u7e96", "289": "\u6beb\u7121\u5dee\u932f\u65e2\u7121\u5dee\u932f\u4e14\u9053\u5373\u4eca\u8def\u982d\u5728\u751a\u9ebc\u8655\u4ee5\u6756", "290": "\u5f15\u9f95\u4e91\u9a40\u76f4\u53bb\u3002", "291": "\u8209\u706b\u4e91\u5875\u7de3\u672a\u8ff7\u60d1\u5fc3\u93e1\u5713\u660e\u771f\u5e38\u7368\u9732\u6b77", "292": "\u6b77\u7121\u751f\u6b7b\u660e\u660e\u7d55\u6709\u8005\u4e00\u5177\u76ae\u56ca\u7121\u4e0b\u4ea4\u4ee3", "293": "\u6240\u4ee5\u5c71\u50e7\u76f4\u5f97\u70ba\u5927\u773e\u70ba\u4f0a\u6dfb\u67f4\u770b\u770b\u706b\u6ec5", "294": "\u7159\u6d88\u5f8c\u5206\u660e\u907a\u4e0b\u4e00\u5806\u7070\u3002", "295": "\u5929\u4e00\u6085\u79aa\u5e2b\u8a9e\u9304\u7d42", "296": "\u6350\u8cb2\u773e\u5584\u82b3\u540d", "297": "\u4fe1\u5b98\u9f8d\u5143\u5409\u6cd5\u540d\u8056\u5b78\u540c\u7de3\u5b89\u6c0f\u852d\u8972\u5fc3\u5be7", "298": "\u4fe1\u58eb\u5468\u5fb7\u5947\u3000\u3000\u5468\u5b9a\u4e00\u3000\u3000\u8d99\u4e16\u5091\u3000\u3000\u9676\u8056\u5091", "299": "\u5468\u5fb7\u7dad\u4edd\u5ba4\u5433\u6c0f\u3000\u3000\u3000\u8d99\u4e16\u52f3\u3000\u3000\u99ac\u8056\u610f", "300": "\u859b\u9580\u694a\u6c0f\u3000\u3000\u5b9a\u6a5f\u3000\u3000\u5f6d\u5947\u624d\u3000\u3000\u8b5a\u842c\u5b78", "301": "\u8d99\u8cb4\u548c\u3000\u3000\u5468\u5b9a\u90a6\u3000\u3000\u5289\u4ef2\u5148\u3000\u3000\u8b5a\u3000\u85fb", "302": "\u8d99\u548c\u3000\u3000\u5468\u3000\u3000\u3000\u5c39\u671d\u9075\u3000\u3000\u8b5a\u3000\u97dc", "303": "\u8d99\u4e16\u83ef\u3000\u3000\u5468\u3000\u3000\u3000\u9127\u8cb4\u751f\u3000\u3000\u8b5a\u3000\u7565", "304": "\u99ac\u3000\u3000\u3000\u3000\u8881\u5927\u7d33", "305": "\u674e\u7199\u6587\u3000\u3000\u3000\u7d39\u6c0f\u5927\u4e58", "306": "\u6bb5\u8ce2\u3000\u3000\u3000\u3000\u55bb\u5927\u4fe1", "307": "\u859b\u570b\u68df\u3000\u3000\u3000\u3000\u3000\u3000\u3000\u6c88\u660e\u5fb7\u7b49\u540c\u523b", "308": "\u6cd5\u55e3\u8056\u975c\u52df\u523b", "309": "\u5929\u9f8d\u6cd5\u5f1f\u5982\u7d14\u5c0d", "310": "\u6d77\u9e7d\u9673\u3000\u856d\u66f8\u3000\u3000\u695e\u56b4\u3000\u590f\u7dad\u5be7\u3000\u502a\u723e\u7e69\u3000\u540c\u942b", "311": "\u5eb7\u7199\u4e01\u4ea5\u5b63\u590f\u3000\u7a40\u65e6\u5609\u8208\u695e\u56b4\u5bfa\u822c\u82e5\u5802\u85cf\u677f"}, "englishTitle": "Jiaxing Canon, Electronic version, No. B470 \u5929\u4e00\u6085\u79aa\u5e2b\u8a9e\u9304", "author": "\u6e05 \u5927\u6085\u8aaa\u3000\u5b78\u611a\u9304\u3000\u6709\u5854\u9298\uff08\u4f9d\u99d2\u672c\u5370\uff09"}