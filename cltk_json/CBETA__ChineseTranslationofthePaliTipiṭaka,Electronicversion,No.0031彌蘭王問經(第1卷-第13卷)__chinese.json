\u537f\u63d0\u51fa\u3002\u6b64\u53ef\u4f9d\u537f\u800c\u89e3\u3002\u300d", "178": "\u300c\u5927\u738b\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a", "179": "\u884c\u4e5e\u52ff\u653e\u9038", "18": "\u7a31\u63da\u81ea\u5fb7\u3002", "180": "\u65bc\u80c3\u9808\u81ea\u5236", "181": "\u7136\uff0c\u53c8\u8a00\uff1a\u300e\u512a\u9640\u5937\uff01\u6211\u6709\u6642\u4ea6\u98df\u7de3\u6eff\u6b64\u9262\u8005\uff0c\u4ea6\u98df\u5f7c\u4ee5\u4e0a\u8005\u3002\u300f\u5927\u738b\uff01\u4f9d\u4e16\u5c0a\u8a00\uff1a", "182": "\u884c\u4e5e\u52ff\u653e\u9038", "183": "\u65bc\u80c3\u9808\u81ea\u5236", "184": "\u8005\u662f\u3014\u8868\u793a\u3015\u672c\u6027\u4e4b\u8a9e\uff0c\u662f\u52dd\u8005\u3001\u725f\u5c3c\u3001\u963f\u7f85\u6f22\u3001\u8f9f\u652f\u4f5b\u3001\u4e00\u5207\u77e5\u8005\u4e4b\u8a00\u3002\u5927\u738b\uff01", "185": "\u65bc\u80c3\u7121\u81ea\u5236\u8005\u5373\u6bba\u751f\u7269\u3001\u4e0d\u8207\u53d6\u3001\u901a\u4ed6\u4eba\u4e4b\u59bb\u3001\u4f5c\u5984\u8a9e\u3001\u98f2\u9152\u3001\u6bba\u6bcd\u3001\u6bba\u7236\u3001\u6bba\u963f", "186": "\u7f85\u6f22\u3001\u7834\u548c\u5408\u50e7\u3001\u4ee5\u60e1\u5fc3\u51fa\u5982\u4f86\u8eab\u8840\u3002\u5927\u738b\uff01\u63d0\u5a46\u9054\u591a\u65bc\u80c3\u7121\u81ea\u5236\uff0c\u7834\u548c\u5408\u50e7\uff0c\u84c4", "187": "\u7a4d\u4e00\u52ab\u9593\uff08\u82e6\u53d7\uff09\u60e1\u696d\u3002\u5927\u738b\uff01\u5982\u662f\uff0c\u898b\u5176\u4ed6\u7a2e\u7a2e\u4e4b\u4e8b\u4f8b\uff0c\u4f9d\u4e16\u5c0a\u8a00\uff1a\u300e\u65bc\u80c3\u9808\u81ea", "188": "\u5236\u3002\u300f\u5927\u738b\uff01\u65bc\u80c3\u81ea\u5236\u8005\uff0c\u4f5c\u56db\u8ae6\u4e4b\u73fe\u89c0\uff0c\u4f5c\u8b49\u56db\u6c99\u9580\u679c\uff0c\u65bc\u56db\u7121\u7919\u89e3\u3001\u4e5d\u6b21\u7b2c\u4f4f\u5b9a\u3001", "189": "\u516d\u795e\u901a\u800c\u5f97\u81ea\u5728\uff0c\u5713\u6210\u5168\u6c99\u9580\u6cd5\u3002\u5927\u738b\uff01\u7f8e\u9ce9\u65bc\u80c3\u6709\u81ea\u5236\uff0c\u9707\u99ed\u4e09\u5341\u4e09\u5929\u754c\uff0c\u4f9b\u5949", "19": "\u65bd\u7f85\uff01\u6211\u662f\u738b", "190": "\u5929\u4e3b\u5e1d\u91cb\u3002\u5927\u738b\uff01\u5982\u662f\uff0c\u898b\u5176\u4ed6\u7a2e\u7a2e\u4e4b\u4e8b\u4f8b\uff0c\u4f9d\u4e16\u5c0a\u8a00\uff1a\u300e\u65bc\u80c3\u9808\u81ea\u5236\u3002\u300f\u5927\u738b\uff01\u7136\uff0c", "191": "\u4f9d\u4e16\u5c0a\u8a00\uff1a\u300e\u512a\u9640\u5937\uff01\u6211\u6709\u6642\u4ea6\u98df\u7de3\u6eff\u6b64\u9262\u8005\uff0c\u4ea6\u98df\u5f7c\u4ee5\u4e0a\u8005\u3002\u300f\u4f5c\u4e86\u61c9\u4f5c\uff0c\u5b8c\u4e86\u6240", "192": "\u4f5c\uff0c\u6210\u5c31\u76ee\u7684\uff0c\u5b8c\u6210\u7a76\u7adf\u7121\u969c\u7919\uff0c\u4f9d\u4e00\u5207\u77e5\u8005\u3001\u7368\u5b58\u8005\u4e4b\u5982\u4f86\u800c\u5c0d\u81ea\u5df1\u800c\u8a00\u3002\u5927", "193": "\u738b\uff01\u8b6c\u5982\u5c0d\u65bc\u4e0b\u75e2\u3001\u5614\u5410\u3001\u6d63\u8178\u4e4b\u75c5\u4eba\uff0c\u53ef\u671b\u9069\u7576\u4e4b\u8655\u7406\u3002\u5927\u738b\uff01\u5982\u662f\uff0c\u6709\u7169\u60f1\uff0c", "194": "\u4e0d\u898b\u56db\u8ae6\u8005\uff0c\u65bc\u80c3\u9808\u81ea\u5236\u3002\u5927\u738b\uff01\u8b6c\u5982\u6709\u5149\u8f1d\uff0c\u6709\u5149\u6fa4\uff0c\u8cea\u826f\u660e\u6f84\u4e4b\u6469\u5c3c\u73e0\u8005\u4e0d\u7528", "195": "\u78e8\u3001\u7422\u3001\u6de8\u5316\u3002\u5982\u662f\uff0c\u5927\u738b\uff01\u9054\u65bc\u4f5b\u9640\u5883\u754c\u7a76\u7adf\u4e4b\u4f5b\u4e16\u5c0a\uff0c\u4f5c\u6240\u4f5c\u800c\u7121\u969c\u7919\u3002\u300d", "196": "\u300c\u5584\u54c9\uff0c\u5c0a\u8005\u90a3\u5148\uff01\u5f7c\u7136\uff0c\u4e88\u5982\u662f\u8a8d\u53d7\u3002\u300d", "197": "\u7b2c\u4e03\u3000\u6cd5\u5f8b\u96b1\u8986\u4e4b\u554f\u7b2c\u4e03\u3000\u6cd5\u5f8b\u96b1\u8986\u4e4b\u554f", "198": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u5982\u4f86\u5ba3\u793a\u6cd5\u8207\u5f8b\u662f\u4ee4\u986f\u9732\u8005\u5149\u8f1d\uff0c\u96b1", "199": "\u8986\u8005\u4e0d\u5149\u8f1d\u3002\u300f\u7136\uff0c\u53c8\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u8207\u5168\u5f8b\u85cf\u4ee4\u79d8\u533f\u3001\u96b1\u8986\u3002\u5c0a\u8005\u90a3\u5148\uff01\u82e5\u5f8b\u4e4b", "2": "\u7b2c\u4e00\u3000\u8b9a\u8a00\u4e4b\u554f\u7b2c\u4e00\u3000\u8b9a\u8a00\u4e4b\u554f", "20": "\u7121\u4e0a\u4e4b\u6cd5\u738b", "200": "\u5236\u5b9a\u662f\u4ee4\u986f\u9732\u800c\u5149\u8f1d\uff0c\u4eba\u4eba\u5247\u65bc\u52dd\u8005\u4e4b\u6559\uff0c\u5f97\u6b63\u77e5\uff0c\u5f97\u5713\u6210\u3002\u7136\u800c\uff0c\u4f9d\u5982\u4f55\u4e4b\u7406\u7531", "201": "\u800c\u4e00\u5207\u4e4b\u5b78\u3001\u81ea\u5236\u3001\u5236\u5fa1\u3001\u6212\u5fb7\u3001\u6b63\u884c\u4e4b\u5236\u5b9a\uff0c\u7fa9\u5473\u3001\u6cd5\u5473\u3001\u89e3\u812b\u5473\u4e4b\u3014\u5168\u5f8b\u85cf\u4ee4", "202": "\u79d8\u533f\u3001\u96b1\u8986\u8036\uff1f\u3015\u5c0a\u8005\u90a3\u5148\uff01\u82e5\u4f9d\u4e16\u5c0a\u8a00\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u5982\u4f86\u5ba3\u793a\u4e4b\u6cd5\u8207\u5f8b\u662f\u4ee4\u986f\u9732", "203": "\u8005\u5149\u8f1d\uff0c\u96b1\u8986\u8005\u4e0d\u5149\u8f1d\u3002\u300f\u7136\u8005\uff0c\u300e\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u8207\u5168\u5f8b\u85cf\u4ee4\u79d8\u533f\u3001\u96b1\u8986\u300f\u4e4b\u8a00", "204": "\u662f\u90aa\u3002\u82e5\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u8207\u5168\u5f8b\u85cf\u4ee4\u79d8\u533f\u3001\u96b1\u8986\uff0c\u7136\u8005\uff0c\u300e\u8af8\u6bd4\u4e18\uff01\u5982\u4f86\u5ba3\u793a\u4e4b", "205": "\u6cd5\u8207\u5f8b\u662f\u4ee4\u986f\u9732\u8005\u5149\u8f1d\uff0c\u96b1\u8986\u8005\u4e0d\u5149\u8f1d\u300f\u4e4b\u8a00\u662f\u90aa\u3002\u6b64\u4ea6\u5169\u5200\u8ad6\u6cd5\u4e4b\u554f\uff0c\u5411\u537f\u63d0\u51fa\uff0c", "206": "\u6b64\u53ef\u4f9d\u537f\u800c\u89e3\u3002\u300d", "207": "\u300c\u5927\u738b\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u5982\u4f86\u5ba3\u793a\u4e4b\u6cd5\u8207\u5f8b\u662f\u4ee4\u986f\u9732\u8005\u5149\u8f1d\uff0c\u96b1\u8986\u8005", "208": "\u4e0d\u5149\u8f1d\u3002\u300f\u7136\uff0c\u53c8\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u8207\u5168\u5f8b\u85cf\u4ee4\u79d8\u533f\u3001\u96b1\u8986\u3002\u7136\uff0c\u6b64\u975e\u3014\u79d8\u533f\u3015\u65bc\u4e00", "209": "\u5207\u4eba\uff0c\u5b9a\u7d50\u754c\u800c\u79d8\u533f\u3002\u5927\u738b\uff01\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u8005\u4f9d\u4e16\u5c0a\u800c\u6709\u4e09\u7a2e\u4e4b\u5b9a\u7d50\u754c\u800c\u79d8\u533f\u3002", "21": "\u6211\u4f9d\u6cd5\u8f49\u8f2a", "210": "\u4f9d\u53e4\u6614\u8af8\u5982\u4f86\u4e4b\u7fd2\u6163\u800c\u79d8\u533f\uff0c\u70ba\u6cd5\u4e4b\u5c0a\u91cd\u800c\u79d8\u533f\uff0c\u70ba\u6bd4\u4e18\u5730\u4e4b\u5c0a\u91cd\u800c\u79d8\u533f\u3002\u5982\u4f55\u4f9d", "211": "\u53e4\u6614\u8af8\u5982\u4f86\u4e4b\u7fd2\u6163\u800c\u79d8\u533f\u8036\uff1f\u5927\u738b\uff01\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u65bc\u6bd4\u4e18\u4e2d\u8b80\u8aa6\uff0c\u5c0d\u5176\u4ed6\u4e4b\u4eba", "212": "\u5247\u79d8\u533f\uff0c\u6b64\u53e4\u6614\u4e00\u5207\u8af8\u5982\u4f86\u4e4b\u7fd2\u6163\u3002\u5927\u738b\uff01\u8b6c\u5982\u524e\u5e1d\u5229\u4e4b\u653f\u7565\u552f\u524e\u5e1d\u5229\u800c\u884c\uff0c\u662f\u6545\uff0c", "213": "\u70ba\u524e\u5e1d\u5229\u4e16\u754c\u4e4b\u7fd2\u6163\uff0c\u5c0d\u65bc\u5176\u4ed6\u4eba\u5247\u79d8\u533f\u3002\u5927\u738b\uff01\u5982\u662f\uff0c\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u65bc\u6bd4\u4e18", "214": "\u4e2d\u8b80\u8aa6\uff0c\u5c0d\u65bc\u5176\u4ed6\u4eba\u5247\u79d8\u533f\u3002\u5982\u662f\uff0c\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u4f9d\u53e4\u6614\u8af8\u5982\u4f86\u4e4b\u7fd2\u6163\u5b9a\u7d50\u754c", "215": "\u800c\u79d8\u533f\u3002", "216": "\u5982\u4f55\u662f\u6cd5\u4e4b\u5c0a\u91cd\u800c\u79d8\u533f\u8036\uff1f\u5927\u738b\uff01\u6cd5\u662f\u5c0a\u56b4\u3001\u91cd\u5927\u3002\u6b64\u6b63\u884c\u8005\u5f97\u5b8c\u5168\u667a\u3002\u4f9d\u50b3", "217": "\u627f\u6b63\u884c\u8005\u800c\u902e\u5f97\u5176\u5b8c\u5168\u667a\uff0c\u4f9d\u50b3\u627f\u800c\u4e0d\u6b63\u884c\u8005\u4e0d\u902e\u5f97\u3002\u6b64\u771f\u5be6\u6cd5\u3001\u6700\u52dd\u6cd5\u4f9d\u50b3\u627f\u800c", "218": "\u4e0d\u5165\u65bc\u4e0d\u6b63\u884c\u8005\u4e4b\u624b\u4e2d\uff0c\u5247\u4e0d\u8f15\u8996\u3001\u8ce4\u8996\u3001\u8f15\u4fae\u3001\u8f15\u8511\u3001\u5475\u8cac\u3002\u6b64\u4e4b\u771f\u5be6\u6cd5\u3001\u6700\u52dd", "219": "\u6cd5\u4e0d\u5165\u65bc\u60e1\u4eba\u4e4b\u9593\uff0c\u5247\u4e0d\u8f15\u8996\u3001\u8ce4\u8996\u3001\u8f15\u4fae\u3001\u8f15\u8511\u3001\u5475\u8cac\uff0c\u5982\u662f\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80", "22": "\u7121\u9006\u8f49\u4e4b\u8f2a", "220": "\u8aa6\u70ba\u6cd5\u4e4b\u5c0a\u91cd\u5b9a\u7d50\u754c\u800c\u79d8\u533f\u3002\u5927\u738b\uff01\u771f\u6b63\u3001\u6700\u52dd\u3001\u6975\u52dd\u3001\u512a\u7f8e\u3001\u826f\u8cea\u4e4b\u8d64\u6834\u6a80\u6728", "221": "\u5165\u838e\u74e6\u7f85\uff08\u8ce4\u6c11\u4e4b\u8857\uff09\u800c\u8f15\u8996\u8036\uff1f\u5927\u738b\uff01\u5982\u662f\uff0c\u6b64\u771f\u5be6\u6cd5\u3001\u6700\u52dd\u6cd5\u4f9d\u627f\u50b3\u800c\u4e0d\u5165\u65bc", "222": "\u4e0d\u6b63\u884c\u8005\u4e4b\u624b\u4e2d\uff0c\u5247\u4e0d\u8f15\u8996\u3001\u8ce4\u8996\u3001\u8f15\u4fae\u3001\u8f15\u8511\u3001\u5475\u8cac\uff0c\u6b64\u4e4b\u771f\u5be6\u6cd5\u3001\u6700\u52dd\u6cd5\u4e0d\u5165", "223": "\u65bc\u60e1\u4eba\u4e4b\u9593\uff0c\u5247\u4e0d\u8f15\u8996\u3001\u8ce4\u8996\u3001\u8f15\u4fae\u3001\u8f15\u8511\u3001\u5475\u8cac\uff0c\u5982\u662f\uff0c\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u662f\u6cd5", "224": "\u4e4b\u5c0a\u91cd\u5b9a\u7d50\u754c\u800c\u79d8\u533f\u3002", "225": "\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u5982\u4f55\u662f\u6bd4\u4e18\u5730\u4e4b\u5c0a\u91cd\u5b9a\u7d50\u754c\u800c\u79d8\u533f\u8036\uff1f\u5927\u738b\uff01\u6bd4\u4e18\u4e4b\u5730\u4f4d\u662f", "226": "\u7121\u6bd4\u3001\u7121\u91cf\u3001\u7121\u50f9\u3002\u4f55\u4eba\u4ea6\u4e0d\u80fd\u8a55\u50f9\u3001\u6bd4\u8f03\u3001\u8861\u91cf\u3002\u5982\u662f\uff0c\u4f4f\u7acb\u65bc\u6b64\u6bd4\u4e18\u4e4b\u5730\u4f4d\u8005", "227": "\u4e0d\u8207\u4e16\u9593\u7b49\u540c\uff0c\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u552f\u65bc\u6b64\u7b49\u4e4b\u6bd4\u4e18\u4e2d\u800c\u884c\u3002\u5927\u738b\uff01\u8b6c\u5982\u4efb\u4f55\u5b58\u65bc\u4e16", "228": "\u9593\u4e4b\u6216\u4f9d\u3001\u6216\u7d68\u6bef\uff0c\u6216\u8c61\u3001\u99ff\u99ac\u3001\u8eca\u3001\u91d1\u9280\u3001\u6469\u5c3c\u3001\u771f\u73e0\u5973\u5bf6\u7b49\u6216\u5f37\u9152\u4e4b\u6700\u52dd\u3001", "229": "\u6975\u52dd\u8005\u6089\u5c6c\u65bc\u738b\u3002\u5927\u738b\uff01\u65bc\u4e16\u9593\u5584\u901d\u50b3\u627f\u4e4b\u5b78\u3001\u6b63\u884c\u3001\u81ea\u5236\u3001\u6212\u3001\u5f8b\u5100\u4e4b\u5fb7\u6089\u5c6c\u65bc", "23": "\u5927\u738b\uff01\u524d\u8005\u662f\u4e16\u5c0a\u986f\u793a\u6cd5\u3001\u672c\u4f86\u3001\u975e\u507d\u3001\u5be6\u3001\u771f\u4e4b\u672c\u6027\u3001\u7279\u8cea\u3001\u7279\u76f8\u800c\u8aaa\uff1a\u300e\u8af8", "230": "\u6bd4\u4e18\u50e7\u4f3d\u3002\u5982\u662f\uff0c\u6ce2\u7f85\u63d0\u6728\u53c9\u4e4b\u8b80\u8aa6\u662f\u6bd4\u4e18\u5730\u4e4b\u5c0a\u656c\u5b9a\u7d50\u754c\u800c\u79d8\u533f\u3002\u300d", "231": "\u300c\u5584\u54c9\uff0c\u5c0a\u8005\u90a3\u5148\uff01\u5f7c\u7136\uff0c\u4e88\u5982\u662f\u8a8d\u53d7\u3002\u300d", "232": "\u7b2c\u516b\u3000\u5984\u8a9e\u8f15\u91cd\u4e4b\u554f\u7b2c\u516b\u3000\u5984\u8a9e\u8f15\u91cd\u4e4b\u554f", "233": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a\u300e\u6545\u610f\u4e4b\u5984\u8a9e\u662f\u6ce2\u7f85\u5937\u3002\u300f\u7136\uff0c\u53c8\u4f9d\u5982\u4f86\u8a00\uff1a\u300e\u82e5", "234": "\u6545\u610f\u5984\u8a9e\uff0c\u5247\u72af\u8f15\u7f6a\uff0c\u8981\u65bc\u4e00\u6bd4\u4e18\u4e4b\u8655\u544a\u767d\u3002\u300f\u5c0a\u8005\u90a3\u5148\uff01\u4f9d\u4e00\u5984\u8a9e\u800c\u7834\u9580\u8207\u4f9d\u4e00\u5984", "235": "\u8a9e\u800c\u5f97\u6055\uff0c\u6b64\u5169\u8005\u4e4b\u5dee\u5225\u5982\u4f55\uff0c\u5dee\u7570\u5982\u4f55\u3002\u5c0a\u8005\u90a3\u5148\uff01\u82e5\u4f9d\u4e16\u5c0a\u8a00\uff1a\u300e\u6545\u610f\u4e4b\u5984\u8a9e", "236": "\u662f\u6ce2\u7f85\u5937\u3002\u300f\u7136\u8005\uff0c\u300e\u82e5\u6545\u610f\u5984\u8a9e\uff0c\u5247\u72af\u8f15\u7f6a\uff0c\u8981\u65bc\u4e00\u6bd4\u4e18\u4e4b\u8655\u544a\u767d\u300f\u4e4b\u8a00\u662f\u90aa\u3002\u82e5", "237": "\u4f9d\u5982\u4f86\u8a00\uff1a\u300e\u82e5\u6545\u610f\u5984\u8a9e\uff0c\u5247\u72af\u8f15\u7f6a\uff0c\u8981\u65bc\u4e00\u6bd4\u4e18\u4e4b\u8655\u544a\u767d\u3002\u300f\u7136\u8005\uff0c\u300e\u6545\u610f\u4e4b\u5984\u8a9e", "238": "\u662f\u6ce2\u7f85\u5937\u300f\u4e4b\u8a00\u662f\u90aa\u3002\u6b64\u4ea6\u5169\u5200\u8ad6\u6cd5\u4e4b\u554f\uff0c\u5411\u537f\u63d0\u51fa\u3002\u6b64\u53ef\u4f9d\u537f\u800c\u89e3\u3002\u300d", "239": "\u300c\u5927\u738b\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a\u300e\u6545\u610f\u4e4b\u5984\u8a9e\u662f\u6ce2\u7f85\u5937\u3002\u300f\u7136\u800c\uff0c\u53c8\u8a00\uff1a\u300e\u82e5\u6545\u610f\u5984\u8a9e\uff0c", "24": "\u6bd4\u4e18\uff01\u4ed6\u4eba\u8b9a\u8a00\u6211\u6216\u6211\u6cd5\u6216\u6211\u4e4b\u50e7\u4f3d\uff0c\u6c5d\u7b49\u5c0d\u6b64\u4e0d\u70ba\u6176\u559c\u3001\u6b23\u559c\u3001\u618d\u9038\u65bc\u5fc3\u3002\u300f\u53c8\uff0c", "240": "\u5247\u72af\u8f15\u7f6a\uff0c\u8981\u65bc\u4e00\u6bd4\u4e18\u4e4b\u8655\u544a\u767d\u3002\u300f\u7136\uff0c\u6b64\u4f9d\u4e8b\u60c5\u800c\u4ea6\u70ba\u91cd\u7f6a\uff0c\u4ea6\u70ba\u8f15\u7f6a\u3002\u5927\u738b\uff01\u537f", "241": "\u5982\u4f55\u601d\u5f7c\u8036\uff1f\u6b64\u8655\uff0c\u6709\u4e00\u4eba\uff0c\u4ee5\u624b\u6b32\u6253\u4ed6\u4eba\uff0c\u537f\u79d1\u5982\u4f55\u4e4b\u7f6a\u65bc\u5f7c\u8036\uff1f\u300d", "242": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u82e5\u88ab\u5bb3\u8005\u8a00\uff1a\u300e\u6211\u4e0d\u5bb9\u8a31\u3002\u300f\u5247\u5176\u3014\u52a0\u5bb3\u8005\u3015\u4ed8\u7f70\u91d1\u65bc\u5f7c\u4e0d\u5bb9\u8a31\u4e4b", "243": "\u3014\u88ab\u5bb3\u8005\u3015\u3002\u300d", "244": "\u300c\u5927\u738b\uff01\u53c8\u4ed6\u4eba\u4ee5\u624b\u6b32\u6253\u537f\uff0c\u537f\u79d1\u5982\u4f55\u4e4b\u7f70\u65bc\u5f7c\u8036\uff1f\u300d", "245": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u4ee4\u622a\u624b\u8db3\uff0c\u81f3\u982d\u5982\u5272\u7af9\u800c\u5272\uff0c\u61c9\u6c92\u6536\u5bb6\u4e4b\u4e00\u5207\uff0c\u61c9\u81f3\u7236\u6bcd\u5169\u7cfb\u4e03\u4ee3", "246": "\u800c\u6ec5\u7d55\u3002\u300d", "247": "\u300c\u5927\u738b\uff01\u4ee5\u624b\u6253\u4eba\u4e4b\u6642\uff0c\u4ed8\u7f70\u91d1\u4e4b\u8f15\u7f70\u8207\u4ee5\u6253\u537f\u4e4b\u6642\uff0c\u622a\u624b\u8db3\uff0c\u81f3\u982d\u5982\u5272\u7af9\u800c\u5272\uff0c", "248": "\u6c92\u6536\u5bb6\u4e4b\u4e00\u5207\uff0c\u81f3\u7236\u6bcd\u5169\u7cfb\u4e03\u4ee3\u800c\u6ec5\u7d55\u8005\uff0c\u5176\u5dee\u5225\u5982\u4f55\uff0c\u5176\u5dee\u7570\u5982\u4f55\u3002\u300d", "249": "\u300c\u4f9d\u5c0d\u8005\u3014\u4e4b\u7570\u3015\u4e5f\u3002\u300d", "25": "\u65bd\u7f85\u5a46\u7f85\u9580\u4f5c\u5982\u5be6\u4e4b\u8b9a\u8a00\u6642\uff0c\u3014\u4e16\u5c0a\u3015\u6108\u66f4\u7a31\u63da\u81ea\u5fb7\u8a00\uff1a\u300e\u65bd\u7f85\uff01\u6211\u662f\u738b\uff0c\u662f\u7121\u4e0a\u6cd5", "250": "\u300c\u5927\u738b\uff01\u5982\u662f\uff0c\u6545\u610f\u5984\u8a9e\u4e4b\u6642\uff0c\u4f9d\u5c0d\u8005\u3014\u4e4b\u7570\u3015\uff0c\u4ea6\u70ba\u91cd\u7f6a\uff0c\u4ea6\u70ba\u8f15\u7f6a\u3002\u300d", "251": "\u300c\u5584\u54c9\uff0c\u5c0a\u8005\u90a3\u5148\uff01\u5f7c\u7136\uff0c\u4e88\u5982\u662f\u8a8d\u53d7\u3002\u300d", "252": "\u7b2c\u4e5d\u3000\u7121\u4e0a\u91ab\u5e2b\u4e4b\u554f\u7b2c\u4e5d\u3000\u7121\u4e0a\u91ab\u5e2b\u4e4b\u554f", "253": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u6211\u662f\u5a46\u7f85\u9580\uff0c\u8acb\u61c9\u5e38\u6e05\u6de8\u5176\u624b\uff0c\u6301\u6700\u5f8c", "254": "\u8eab\uff0c\u70ba\u7121\u4e0a\u4e4b\u91ab\u5e2b\uff0c\u70ba\u7bad\u91ab\u3002\u300f\u7136\uff0c\u53c8\u4f9d\u4e16\u5c0a\u8a00\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u6211\u5f1f\u5b50\u6bd4\u4e18\u7121\u75c5\u4e2d\u7b2c\u4e00", "255": "\u8005\u662f\u8584\u62d8\u7f85\u3002\u300f\u7136\uff0c\u77e5\u4e16\u5c0a\u4e4b\u8eab\u5c62\u75c5\u800c\u8d77\u3002\u5c0a\u8005\u90a3\u5148\uff01\u82e5\u5982\u4f86\u662f\u7121\u4e0a\u4e4b\u91ab\u5e2b\uff0c\u7136", "256": "\u8005\u300e\u8af8\u6bd4\u4e18\uff01\u6211\u5f1f\u5b50\u6bd4\u4e18\u7121\u75c5\u4e2d\u7b2c\u4e00\u662f\u8584\u62d8\u7f85\u300f\u4e4b\u8a00\u662f\u90aa\u3002\u82e5\u9577\u8001\u8584\u62d8\u7f85\u662f\u7121\u75c5\u7b2c", "257": "\u4e00\uff0c\u7136\u8005\uff0c\u300e\u8af8\u6bd4\u4e18\uff01\u6211\u662f\u5a46\u7f85\u9580\uff0c\u8af8\u61c9\u5e38\u6e05\u6de8\u5176\u624b\uff0c\u6301\u6700\u5f8c\u8eab\uff0c\u70ba\u7121\u4e0a\u4e4b\u91ab\u5e2b\uff0c\u70ba", "258": "\u7bad\u91ab\u300f\u4e4b\u8a00\u662f\u90aa\u3002\u6b64\u4ea6\u5169\u5200\u8ad6\u6cd5\u4e4b\u554f\uff0c\u5411\u537f\u63d0\u51fa\uff0c\u6b64\u53ef\u4f9d\u537f\u800c\u89e3\u3002\u300d", "259": "\u300c\u5927\u738b\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u6211\u662f\u5a46\u7f85\u9580\uff0c\u8acb\u61c9\u5e38\u6e05\u6de8\u5176\u624b\uff0c\u6301\u6700\u5f8c\u8eab\uff0c", "26": "\u738b\u3002\u300f\u8005\uff0c\u6b64\u4e0d\u70ba\u5229\u5f97\uff0c\u4e0d\u70ba\u540d\u8072\uff0c\u4e0d\u70ba\u5f97\u3014\u9ee8\u3015\uff0c\u4e0d\u6b32\u70ba\u5f1f\u5b50\uff0c\u300e\u5982\u662f\uff0c\u4e09\u767e\u4f4d\u4e4b\u5b78", "260": "\u662f\u7121\u4e0a\u4e4b\u91ab\u5e2b\uff0c\u662f\u7bad\u91ab\u3002\u300f\u7136\uff0c\u4e16\u5c0a\u53c8\u8a00\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u6211\u5f1f\u5b50\u6bd4\u4e18\u7121\u75c5\u4e2d\u7b2c\u4e00\u8005\u662f\u8584", "261": "\u62d8\u7f85\u3002\u300f\u5927\u738b\uff01\u7136\uff0c\u6b64\u53d7\u7531\u5916\u4e4b\u50b3\u627f\uff0c\u6210\u5c31\u3001\u901a\u66c9\u8005\u4e4b\u4e2d\uff0c\u6709\u95dc\u5b58\u65bc\u3014\u67d0\u7279\u6027\u3015\u81ea\u8eab", "262": "\u4e4b\u4e2d\u800c\u8a00\u3002\u5927\u738b\uff01\u4e16\u5c0a\u4e4b\u5f1f\u5b50\u6709\u4f47\u7acb\u3001\u7d93\u884c\u8005\uff0c\u5f7c\u7b49\u4f9d\u4f47\u7acb\u8207\u7d93\u884c\u800c\u904e\u665d\u591c\u3002\u5927\u738b\uff01", "263": "\u7136\uff0c\u4e16\u5c0a\u4f9d\u4f47\u7acb\u3001\u7d93\u884c\u3001\u5750\u3001\u81e5\u800c\u904e\u665d\u591c\u3002\u5927\u738b\uff01\u51e1\u5f7c\u7b49\u6bd4\u4e18\u4e4b\u4f47\u7acb\u3001\u7d93\u884c\u8005\u65bc\u5176", "264": "\u7279\u6b8a\u4e4b\u5fb7\u800c\u512a\u3002\u5927\u738b\uff01\u4e16\u5c0a\u4e4b\u5f1f\u5b50\u6709\u4e00\u5750\u98df\u8005\uff0c\u5f7c\u7b49\u96d6\u7136\u8207\u751f\u547d\u6709\u95dc\u4ea6\u4e0d\u53d6\u4e8c\u5ea6\u98df\u3002", "265": "\u5927\u738b\uff01\u7136\uff0c\u4e16\u5c0a\u56e0\u7a7a\u8179\u800c\u4e8c\u5ea6\u98df\u3002\u5927\u738b\uff01\u51e1\u5f7c\u7b49\u6bd4\u4e18\u4e4b\u4e00\u5750\u98df\u8005\uff0c\u65bc\u5176\u7279\u6b8a\u4e4b\u5fb7\u800c", "266": "\u512a\u3002\u5927\u738b\uff01\u95dc\u65bc\u6b64\u8a00\u6709\u591a\u7a2e\u4e4b\u4e8b\u4f8b\u3002\u7136\uff0c\u5927\u738b\uff01\u4e16\u5c0a\u65bc\u6212\u3001\u5b9a\u3001\u6167\u3001\u89e3\u812b\u3001\u89e3\u812b\u77e5", "267": "\u898b\u3001\u5341\u529b\u3001\u56db\u7121\u6240\u754f\u3001\u5341\u516b\u4f5b\u6cd5\u3001\u65bc\u4f5b\u9640\u4e4b\u5168\u5883\u754c\u800c\u70ba\u7121\u4e0a\u3002\u95dc\u65bc\u6b64\u8a00\uff1a\u300e\u8af8\u6bd4\u4e18\uff01", "268": "\u6211\u662f\u5a46\u7f85\u9580\uff0c\u8acb\u61c9\u5e38\u6e05\u6de8\u5176\u624b\uff0c\u6301\u6700\u5f8c\u8eab\uff0c\u662f\u7121\u4e0a\u4e4b\u91ab\u5e2b\uff0c\u662f\u7bad\u91ab\u3002\u300f\u5927\u738b\uff01\u8b6c\u5982\u65bc", "269": "\u4e16\u9593\uff0c\u67d0\u4eba\u751f\u5584\uff0c\u67d0\u4eba\u6709\u8ca1\uff0c\u67d0\u4eba\u6709\u667a\uff0c\u67d0\u4eba\u6709\u85dd\uff0c\u67d0\u4eba\u70ba\u52c7\uff0c\u67d0\u4eba\u70ba\u8ce2\u3002\u7136\uff0c\u738b", "27": "\u7ae5\u901a\u9054\u6b64\u6cd5\u300f\uff0c\u4f9d\u5f7c\u54c0\u610d\u3001\u60b2\u6190\u3001\u9952\u76ca\u3002\u5982\u662f\uff0c\u6108\u66f4\u7a31\u63da\u81ea\u5fb7\u3002", "270": "\u512a\u65bc\u5f7c\u7b49\u4e00\u5207\uff0c\u70ba\u5f7c\u7b49\u4e2d\u4e4b\u6700\u4e0a\u3002\u5927\u738b\uff01\u5982\u662f\uff0c\u4e16\u5c0a\u662f\u5f7c\u7b49\u6709\u60c5\u4e2d\u4e4b\u7b2c\u4e00\uff0c\u662f\u6700\u9577\uff0c", "271": "\u662f\u6700\u52dd\u3002\u5c0a\u8005\u8584\u62d8\u7f85\u70ba\u7121\u75c5\u8005\u4f9d\u3014\u65bc\u524d\u751f\u3015\u4e4b\u6c7a\u610f\u3002\u5927\u738b\uff01\u6700\u5d07\u9ad8\u4e4b\u4e16\u5c0a\u80c3\u75c5\u751f", "272": "\u8d77\u6642\uff0c\u53c8\u6bd8\u5a46\u5c38\u4e16\u5c0a\u8207\u516d\u842c\u516b\u5343\u4e4b\u6bd4\u4e18\u8349\u82b1\u75c5\u751f\u8d77\u4e4b\u6642\uff0c\u81ea\u8eab\u4ea6\u70ba\u82e6\u884c\u8005\uff0c\u5f7c\u4ee5\u7a2e", "273": "\u7a2e\u4e4b\u85e5\u9664\u5176\u75c5\uff0c\u3014\u4f9d\u6b64\u5f7c\u3015\u70ba\u7121\u75c5\u3002\u6545\u8a00\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u6211\u5f1f\u5b50\u6bd4\u4e18\u7121\u75c5\u4e2d\u7b2c\u4e00\u662f\u8584\u62d8", "274": "\u7f85\u3002\u300f\u5927\u738b\uff01\u4e16\u5c0a\u75c5\u751f\u8d77\u4e4b\u6642\u6216\u4e0d\u751f\u8d77\u4e4b\u6642\uff0c\u6301\u982d\u9640\u652f\u4e4b\u6642\u6216\u4e0d\u6301\u4e4b\u6642\uff0c\u7121\u7b49\u65bc\u4e16\u5c0a", "275": "\u4e4b\u6709\u60c5\u3002\u5927\u738b\uff01\u4f9d\u5929\u4e2d\u5929\u4e4b\u4e16\u5c0a\u65bc\u76f8\u61c9\u90e8\u4e4b\u6700\u52dd\u5bc4\u8207\u5982\u662f\u8aaa\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u6216\u7121\u8db3\u3001", "276": "\u6216\u4e8c\u8db3\u3001\u6216\u56db\u8db3\u3001\u6216\u591a\u8db3\u3001\u6216\u6709\u8272\u3001\u6216\u7121\u8272\u3001\u6216\u6709\u60f3\u3001\u6216\u7121\u60f3\u3001\u6216\u975e\u60f3\u975e\u975e\u60f3\u4e4b\u4e00", "277": "\u5207\u6709\u60c5\u4e4b\u4e2d\uff0c\u8a00\u61c9\u4f9b\u3001\u6b63\u7b49\u89ba\u8005\u70ba\u5f7c\u7b49\u4e4b\u7b2c\u4e00\u3002\u300f\u300d", "278": "\u300c\u5584\u54c9\uff0c\u5c0a\u8005\u90a3\u5148\uff01\u5f7c\u7136\uff0c\u4e88\u5982\u662f\u8a8d\u53d7\u3002\u300d", "28": "\u65bd\u7f85\uff01\u6211\u662f\u738b", "29": "\u7121\u4e0a\u4e4b\u6cd5\u738b", "3": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a\u300e\u8af8\u6bd4\u4e18\uff01\u4ed6\u4eba\u8b9a\u8a00\u6211\u6216\u6211\u6cd5\u6216\u6211\u4e4b\u50e7\u4f3d\uff0c\u6c5d\u7b49", "30": "\u4f9d\u6cd5\u800c\u8f49\u8f2a", "31": "\u7121\u9006\u8f49\u4e4b\u8f2a\u300d", "32": "\u300c\u5584\u54c9\uff0c\u5c0a\u8005\u90a3\u5148\uff01\u5f7c\u7136\uff0c\u4e88\u5982\u662f\u8a8d\u53d7\u3002\u300d", "33": "\u7b2c\u4e8c\u3000\u4e0d\u5bb3\u8207\u6298\u4f0f\u4e4b\u554f\u7b2c\u4e8c\u3000\u4e0d\u5bb3\u8207\u6298\u4f0f\u4e4b\u554f", "34": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a", "35": "\u65bc\u6b64\u4e16\u4e0d\u5bb3\u4ed6\u4eba", "36": "\u5feb\u6a02\u89aa\u5207\u65bc\u4ed6\u4eba", "37": "\u7136\uff0c\u53c8\u8a00\uff1a", "38": "\u61c9\u6298\u4f0f\u8005\u6298\u4f0f", "39": "\u61c9\u651d\u53d7\u8005\u651d\u53d7", "4": "\u5c0d\u6b64\u70ba\u6176\u559c\u3001\u6b23\u559c\uff0c\u618d\u9038\u65bc\u5fc3\u3002\u300f\u7136\uff0c\u53c8\u5982\u4f86\u5c0d\u65bd\u7f85\u5a46\u7f85\u9580\u4f5c\u5982\u5be6\u4e4b\u8b9a\u8a00\u6642\uff0c\u6176\u559c\u3001", "40": "\u5c0a\u8005\u90a3\u5148\uff01\u8a00\u6298\u4f0f\u8005\u662f\u622a\u624b\u3001\u622a\u8db3\u3001\u6bc6\u6253\u3001\u7e1b\u7d81\u3001\u6832\u554f\u3001\u6b7b\u5211\u3001\u4e0d\u65b7\u52a0\u4ee5\u50b7\u5bb3\u3002", "41": "\u6b64\u8a00\u4e0d\u76f8\u61c9\u65bc\u4e16\u5c0a\uff0c\u53c8\u4e16\u5c0a\u4e0d\u76f8\u61c9\u65bc\u6b64\u8a00\u3002\u5c0a\u8005\u90a3\u5148\uff01\u82e5\u4f9d\u4e16\u5c0a\u8a00\uff1a", "42": "\u65bc\u6b64\u4e16\u4e0d\u5bb3\u4ed6\u4eba", "43": "\u5feb\u6a02\u89aa\u5207\u65bc\u4ed6\u4eba", "44": "\u5982\u662f\u8a00\u8005\uff0c\u7136\uff0c", "45": "\u61c9\u6298\u4f0f\u8005\u6298\u4f0f", "46": "\u61c9\u651d\u53d7\u8005\u651d\u53d7", "47": "\u4e4b\u8a00\u662f\u90aa\u3002\u82e5\u4f9d\u4e16\u5c0a\u8a00\uff1a", "48": "\u61c9\u6298\u4f0f\u8005\u6298\u4f0f", "49": "\u61c9\u651d\u53d7\u8005\u651d\u53d7", "5": "\u6b23\u559c\u3001\u618d\u9038\uff0c\u6108\u66f4\u7a31\u63da\u81ea\u5fb7\u3002", "50": "\u5982\u662f\u8a00\u8005\uff0c\u7136\uff0c", "51": "\u65bc\u6b64\u4e16\u4e0d\u5bb3\u4ed6\u4eba", "52": "\u5feb\u6a02\u89aa\u5207\u65bc\u4ed6\u4eba", "53": "\u4e4b\u8a00\u70ba\u90aa\u3002\u6b64\u4ea6\u5169\u5200\u8ad6\u6cd5\u4e4b\u554f\uff0c\u5411\u537f\u63d0\u51fa\uff0c\u6b64\u53ef\u4f9d\u537f\u800c\u89e3\u3002\u300d", "54": "\u300c\u5927\u738b\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a", "55": "\u65bc\u6b64\u4e16\u4e0d\u5bb3\u4ed6\u4eba", "56": "\u5feb\u6a02\u89aa\u5207\u65bc\u4ed6\u4eba", "57": "\u7136\uff0c\u53c8\u4f9d\u4e16\u5c0a\u8a00\uff1a", "58": "\u61c9\u6298\u4f0f\u8005\u6298\u4f0f", "59": "\u61c9\u651d\u53d7\u8005\u651d\u53d7", "6": "\u65bd\u7f85\uff01\u6211\u662f\u738b", "60": "\u65bc\u6b64\u4e16\u4e0d\u5bb3\u65bc\u4ed6\u4eba\u8005\uff0c\u5927\u738b\uff01\u6b64\u662f\u4e00\u5207\u5982\u4f86\u6240\u8b9a\u53ef\u3002\u6b64\u662f\u5176\u6559\u8aa1\uff0c\u6b64\u662f\u5176\u8aaa\u6cd5\u3002", "61": "\u5927\u738b\uff01\u6cd5\u662f\u4ee5\u4e0d\u5bb3\u70ba\u7279\u76f8\u3002\u6b64\u662f\u3014\u793a\u3015\u5176\u672c\u6027\u4e4b\u8a00\u3002\u5927\u738b\uff01\u7136\uff0c\u5982\u4f86\u8a00\uff1a", "62": "\u61c9\u6298\u4f0f\u8005\u6298\u4f0f", "63": "\u61c9\u651d\u53d7\u8005\u651d\u53d7", "64": "\u6b64\u662f\u5982\u6b21\u4e4b\u610f\uff0c\u5927\u738b\uff01\u6389\u8209\u5fc3\u662f\u61c9\u6298\u4f0f\uff0c\u9000\u7e2e\u5fc3\u662f\u61c9\u651d\u53d7\u3002\u4e0d\u5584\u5fc3\u662f\u61c9\u6298\u4f0f\uff0c\u5584", "65": "\u5fc3\u662f\u61c9\u651d\u53d7\u3002\u975e\u5982\u7406\u4f5c\u610f\u662f\u61c9\u6298\u4f0f\uff0c\u5982\u7406\u4f5c\u610f\u662f\u61c9\u651d\u53d7\u3002\u90aa\u884c\u662f\u61c9\u6298\u4f0f\uff0c\u6b63\u884c\u662f\u61c9", "66": "\u651d\u53d7\u3002\u975e\u8056\u662f\u61c9\u6298\u4f0f\uff0c\u8056\u662f\u61c9\u651d\u53d7\u3002\u76dc\u8cca\u662f\u61c9\u6298\u4f0f\uff0c\u975e\u76dc\u8cca\u662f\u61c9\u651d\u53d7\u3002\u300d", "67": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u5f7c\u7136\u3002\u7136\uff0c\u4eca\u537f\u6b78\u6211\u4e4b\u3014\u8ad6\u3015\u9ede\u3002\u5373\u4e88\u4e4b\u554f\uff0c\u5230\u9054\u4e88\u4e4b\u554f\u984c\u3002\u5c0a\u8005", "68": "\u90a3\u5148\uff01\u76dc\u8cca\u662f\u5982\u4f55\u4f9d\u6298\u4f0f\u8005\u800c\u6298\u4f0f\u8036\uff1f\u300d", "69": "\u300c\u5927\u738b\uff01\u76dc\u8cca\u662f\u4f9d\u6298\u4f0f\u8005\u61c9\u5982\u662f\u4e4b\u6298\u4f0f\u3002\u5475\u8cac\u8005\u61c9\u5475\u8cac\uff0c\u8655\u7f70\u8005\u61c9\u8655\u7f70\uff0c\u653e\u9010\u8005", "7": "\u7121\u4e0a\u4e4b\u6cd5\u738b", "70": "\u61c9\u653e\u9010\uff0c\u7e1b\u7d81\u8005\u61c9\u7e1b\u7d81\uff0c\u6b7b\u5211\u8005\u61c9\u6b7b\u5211\u3002\u300d", "71": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u4ee5\u6b7b\u5211\u76dc\u8cca\u662f\u8af8\u5982\u4f86\u800c\u8b9a\u53ef\u8036\uff1f\u300d", "72": "\u300c\u5927\u738b\uff01\u4e0d\u7136\u3002\u300d", "73": "\u300c\u4f55\u6545\uff0c\u76dc\u8cca\u4e4b\u61c9\u6559\u8aa1\u8005\u662f\u5982\u4f86\u6240\u8b9a\u53ef\u8036\uff1f\u300d", "74": "\u300c\u5927\u738b\uff01\u4e00\u5207\u4e4b\u6b7b\u5211\u8005\uff0c\u5f7c\u975e\u4f9d\u5982\u4f86\u4e4b\u8b9a\u53ef\u800c\u6b7b\u5211\uff0c\u4f9d\u81ea\u5df1\u4e4b\u6240\u884c\u800c\u6b7b\u5211\u3002\u7136\uff0c", "75": "\u5f7c\u6b32\u53d7\u6cd5\u4e4b\u6559\u8aa1\u3002\u5927\u738b\uff01\u7121\u8f9c\u3001\u7121\u7f6a\u3001\u884c\u65bc\u8857\u9053\u4e4b\u4eba\u4f9d\u6709\u601d\u91cf\u4e4b\u4eba\u800c\u6bba\u662f\u53ef\u80fd\u8036\uff1f\u300d", "76": "\u300c\u5c0a\u8005\uff01\u4e0d\u7136\u3002\u300d", "77": "\u300c\u5927\u738b\uff01\u4f9d\u5982\u4f55\u4e4b\u7406\u7531\u8036\uff1f\u300d", "78": "\u300c\u5c0a\u8005\uff01\u662f\u7121\u8f9c\u4e4b\u6545\u3002\u300d", "79": "\u300c\u5927\u738b\uff01\u5982\u662f\uff0c\u76dc\u8cca\u662f\u975e\u4f9d\u5982\u4f86\u4e4b\u8b9a\u53ef\u800c\u6bba\uff0c\u5f7c\u4f9d\u81ea\u5df1\u4e4b\u6240\u884c\u800c\u6bba\u3002\u4f9d\u6b64\uff0c\u6559\u8aa1", "8": "\u6211\u4f9d\u6cd5\u8f49\u8f2a", "80": "\u662f\u72af\u4f55\u904e\u8036\uff1f\u300d", "81": "\u300c\u5c0a\u8005\uff01\u4e0d\u7136\u3002\u300d", "82": "\u300c\u5927\u738b\uff01\u7136\u8005\uff0c\u5982\u4f86\u4e4b\u6559\u8aa1\u662f\u6b63\u6559\u8aa1\u3002\u300d", "83": "\u300c\u5584\u54c9\uff0c\u5c0a\u8005\u90a3\u5148\uff01\u5f7c\u7136\uff0c\u4e88\u5982\u662f\u8a8d\u53d7\u3002\u300d", "84": "\u7b2c\u4e09\u3000\u6bd4\u4e18\u9000\u53bb\u4e4b\u554f\u7b2c\u4e09\u3000\u6bd4\u4e18\u9000\u53bb\u4e4b\u554f", "85": "\u300c\u5c0a\u8005\u90a3\u5148\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a\u300e\u6211\u4e0d\u5fff\u6012\uff0c\u9060\u96e2\u9811\u8ff7\u3002\u300f\u7136\uff0c\u5982\u4f86\u4ee4\u9577\u8001\u820d", "86": "\u5229\u5f17\u3001\u76ee\u728d\u9023\u53ca\u6703\u773e\u4ff1\u9000\u53bb\u3002\u5c0a\u8005\u90a3\u5148\uff01\u5982\u4f86\u5fff\u6012\u800c\u4ee4\u6703\u773e\u9000\u53bb\u8036\uff1f\u6216\u6eff\u8db3\u800c\u4eca\u3014\u6703", "87": "\u773e\u3015\u9000\u53bb\u8036\uff1f\u6700\u521d\uff0c\u537f\u61c9\u77e5\u6b64\u70ba\u5982\u662f\u5982\u662f\u3002\u5c0a\u8005\u90a3\u5148\uff01\u82e5\u5fff\u6012\u800c\u4ee4\u6703\u773e\u9000\u53bb\uff0c\u7136", "88": "\u8005\uff0c\u300e\u5982\u4f86\u5fff\u6012\u672a\u65b7\u300f\u3002\u82e5\u6eff\u8db3\u800c\u4ee4\u3014\u6703\u773e\u3015\u9000\u53bb\uff0c\u7136\u8005\uff0c\u300e\u5982\u4f86\u5c0d\u7121\u6839\u64da\u4e4b\u4e8b\uff0c\u4e0d\u77e5", "89": "\u3014\u4e8b\u5be6\u3015\u800c\u4ee4\u3014\u6703\u773e\u3015\u9000\u53bb\u300f\u3002\u6b64\u4ea6\u5169\u5200\u8ad6\u6cd5\u4e4b\u554f\u3002\u6b64\u53ef\u4f9d\u537f\u800c\u89e3\u3002\u300d", "9": "\u7121\u9006\u8f49\u4e4b\u8f2a", "90": "\u300c\u5927\u738b\uff01\u4f9d\u4e16\u5c0a\u5982\u662f\u8aaa\uff1a\u300e\u6211\u4e0d\u5fff\u6012\uff0c\u9060\u96e2\u9811\u8ff7\u3002\u300f\u7136\uff0c\u53c8\u4ee4\u9577\u8001\u820d\u5229\u5f17\u3001\u76ee\u728d\u9023", "91": "\u4e0a\u9996\u4e4b\u6703\u773e\u9000\u53bb\u3002\u7136\uff0c\u6b64\u975e\u4f9d\u5fff\u6012\u800c\u4ee4\u3014\u6703\u773e\u3015\u9000\u53bb\u3002\u5927\u738b\uff01\u65bc\u6b64\uff0c\u6709\u67d0\u4eba\uff0c\u70ba\u5927", "92": "\u5730\u4e4b\u6a39\u6839\u3001\u6216\u77f3\u6216\u792b\u3001\u6216\u676d\u3001\u6216\u7834\u7247\u3001\u6216\u51f9\u51f8\u4e4b\u5730\u963b\u7919\u800c\u8dcc\u5012\u3002\u5927\u738b\uff01\u5927\u5730\u5fff\u6012", "93": "\u800c\u4f7f\u5f7c\u8dcc\u5012\u8036\uff1f\u300d", "94": "\u300c\u5c0a\u8005\uff01\u4e0d\u7136\u3002\u5927\u5730\u4ea6\u4e0d\u5fff\u6012\uff0c\u4ea6\u4e0d\u6b23\u559c\uff0c\u5927\u5730\u812b\u96e2\u611b\u597d\u8207\u5acc\u60e1\u3002\u5f7c\u4e0d\u6ce8\u610f\u4e4b\u6545\uff0c", "95": "\u963b\u7919\u800c\u8dcc\u5012\u3002\u5927\u738b\uff01\u5982\u662f\uff0c\u5982\u4f86\u4ea6\u4e0d\u5fff\u6012\uff0c\u4ea6\u4e0d\u6b23\u559c\uff0c\u61c9\u4f9b\u3001\u6b63\u7b49\u89ba\u8005\u4e4b\u5982\u4f86\u812b\u96e2", "96": "\u611b\u597d\u8207\u5acc\u60e1\u3002\u5f7c\u7b49\u4f9d\u81ea\u8eab\u4e4b\u6240\u4f5c\uff0c\u4f9d\u81ea\u5df1\u4e4b\u7f6a\u800c\u9000\u53bb\u3002\u5927\u738b\uff01\u53c8\u8b6c\u5982\u5927\u6d77\u4e0d\u5171\u5b58\u5c4d", "97": "\u9ab8\uff0c\u65bc\u5927\u6d77\u4e2d\u4e4b\u8c9d\u3001\u6c34\u8349\u3001\u7da0\u8449\u8349\u3001\u5c4d\u9ab8\uff0c\u8fc5\u901f\u88ab\u63a8\u4e0a\u5cb8\u3002\u5927\u738b\uff01\u5927\u6d77\u5fff\u6012\u800c\u5c07\u5f7c", "98": "\u7b49\u63a8\u4e0a\u5cb8\u8036\uff1f\u300d", "99": "\u300c\u5c0a\u8005\uff01\u4e0d\u7136\u3002\u5927\u6d77\u4ea6\u4e0d\u5fff\u6012\uff0c\u4ea6\u4e0d\u6b23\u559c\u3002\u5927\u6d77\u812b\u96e2\u611b\u597d\u8207\u5acc\u60e1\u3002\u300d"}, "originalTitle": "Chinese Translation of the Pali Tipi\u1e6daka, Electronic version, No. 0031 \u5f4c\u862d\u738b\u554f\u7d93(\u7b2c1\u5377-\u7b2c13\u5377)", "englishTitle": "Chinese Translation of the Pali Tipi\u1e6daka, Electronic version, No. 0031 \u5f4c\u862d\u738b\u554f\u7d93(\u7b2c1\u5377-\u7b2c13\u5377)", "author": "\u90ed\u54f2\u5f70\u8b6f", "language": "chinese", "source": "CBETA"}